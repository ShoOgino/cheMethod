    @Override
    public boolean equals(Object obj) {
      if (this == obj) {
        return true;
      }
      if (!(obj instanceof Key)) {
        return false;
      }
      final Key other = (Key) obj;
      return Objects.equals(user, other.user)
          && Objects.equals(domain, other.domain)
          && Objects.equals(instance, other.instance);
    }

