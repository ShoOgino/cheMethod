  private boolean isWsAgentStarted(WorkspaceImpl workspace) {
    if (workspace == null) {
      return false;
    }
    RuntimeImpl runtime = workspace.getRuntime();
    if (runtime == null) {
      return false;
    }

    Optional<MachineImpl> devMachine = runtime.getDevMachine();

    return devMachine.isPresent();
  }

