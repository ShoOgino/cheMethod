  public static SnapshotImpl createSnapshot(String snapshotId, String workspaceId) {
    return new SnapshotImpl(
        snapshotId,
        "type",
        null,
        System.currentTimeMillis(),
        workspaceId,
        snapshotId + "_description",
        true,
        "dev-machine",
        snapshotId + "env-name");
  }

