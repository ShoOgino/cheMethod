  public MachineStatusChangedEvent(
      String workspaceId,
      String machineId,
      String machineName,
      boolean dev,
      EventType eventType,
      String errorMessage) {
    this.workspaceId = workspaceId;
    this.machineId = machineId;
    this.machineName = machineName;
    this.dev = dev;
    this.eventType = eventType;
    this.errorMessage = errorMessage;
  }

