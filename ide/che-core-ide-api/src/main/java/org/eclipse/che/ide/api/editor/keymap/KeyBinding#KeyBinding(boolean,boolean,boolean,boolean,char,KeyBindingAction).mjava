  public KeyBinding(
      final boolean control,
      final boolean shift,
      final boolean alt,
      final boolean cmd,
      final char character,
      final KeyBindingAction action) {
    this.control = control;
    this.shift = shift;
    this.alt = alt;
    this.cmd = cmd;
    this.keyCodeNumber = null;
    this.character = String.valueOf(character);
    this.action = action;
    this.type = "keypress";
  }

