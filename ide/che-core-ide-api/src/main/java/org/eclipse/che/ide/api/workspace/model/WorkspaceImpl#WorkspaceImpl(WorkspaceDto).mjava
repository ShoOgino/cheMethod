  public WorkspaceImpl(WorkspaceDto workspace) {
    this(
        workspace.getId(),
        workspace.getNamespace(),
        workspace.getConfig(),
        workspace.getRuntime(),
        workspace.getAttributes(),
        workspace.isTemporary(),
        workspace.getStatus(),
        workspace.getLinks(),
        workspace.getRuntime() != null ? workspace.getRuntime().getMachineToken() : null);
  }

