        @Override
        public boolean equals(Object o) {
            if (this == o) {
                return true;
            }

            if (!(o instanceof ApplicableContext)) {
                return false;
            }

            ApplicableContext other = (ApplicableContext)o;

            return workspaceApplicable == other.workspaceApplicable &&
                   Objects.equals(projects, other.projects);
        }

