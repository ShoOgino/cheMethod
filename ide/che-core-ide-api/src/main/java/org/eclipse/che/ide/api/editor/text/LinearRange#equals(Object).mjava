  @Override
  public boolean equals(Object obj) {
    if (this == obj) {
      return true;
    }
    if (obj == null) {
      return false;
    }
    if (getClass() != obj.getClass()) {
      return false;
    }
    LinearRange other = (LinearRange) obj;
    if (length != other.length) {
      return false;
    }
    if (startOffset != other.startOffset) {
      return false;
    }
    return true;
  }

