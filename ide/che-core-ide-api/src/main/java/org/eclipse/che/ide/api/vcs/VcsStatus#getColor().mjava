    public String getColor() {
        switch (this) {
            case UNTRACKED:
                return Style.getVcsStatusUntrackedColor();
            case MODIFIED:
                return Style.getVcsStatusModifiedColor();
            case ADDED:
                return Style.getVcsStatusAddedColor();
            case NOT_MODIFIED:
                return Style.getMainFontColor();
            default:
                return null;
        }
    }

