    /** Returns next page that may be shown. */
    @Nullable
    private WizardPage<T> getNextPage() {
        while (++currentPageIndex < wizardPages.size()) {
            WizardPage<T> page = wizardPages.get(currentPageIndex);
            if (!page.canSkip()) {
                return page;
            }
        }
        return null;
    }

