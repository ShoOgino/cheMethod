    @Override
    protected void dispatch(MachineStateEvent.Handler handler) {
        switch (machineAction) {
            case CREATING:
                handler.onMachineCreating(this);
                break;
            case RUNNING:
                handler.onMachineRunning(this);
                break;
            case DESTROYED:
                handler.onMachineDestroyed(this);
                break;
        }
    }

