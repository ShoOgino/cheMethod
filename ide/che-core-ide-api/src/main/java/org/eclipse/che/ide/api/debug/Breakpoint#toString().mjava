  @Override
  public String toString() {
    StringBuilder builder = new StringBuilder();
    builder
        .append("Breakpoint [lineNumber=")
        .append(lineNumber)
        .append(", type=")
        .append(type)
        .append(", active=")
        .append(active)
        .append(", path=")
        .append(path)
        .append("]");
    return builder.toString();
  }

