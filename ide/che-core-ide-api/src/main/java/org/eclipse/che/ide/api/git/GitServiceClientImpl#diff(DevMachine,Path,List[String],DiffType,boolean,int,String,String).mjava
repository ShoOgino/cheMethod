    @Override
    public Promise<String> diff(DevMachine devMachine,
                                Path project,
                                List<String> fileFilter,
                                DiffType type,
                                boolean noRenames,
                                int renameLimit,
                                String commitA,
                                String commitB) {
        return diff(project, fileFilter, type, noRenames, renameLimit, commitA, commitB, false).send(new StringUnmarshaller());
    }

