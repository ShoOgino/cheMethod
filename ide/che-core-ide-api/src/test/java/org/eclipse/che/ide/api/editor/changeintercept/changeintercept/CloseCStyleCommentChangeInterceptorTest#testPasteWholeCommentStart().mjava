  @Test
  public void testPasteWholeCommentStart() {
    final TextChange input =
        new TextChange.Builder()
            .from(new TextPosition(0, 0))
            .to(new TextPosition(1, 2))
            .insert("/**\n *")
            .build();
    final TextChange output = interceptor.processChange(input, document);
    assertNull(output);
  }

