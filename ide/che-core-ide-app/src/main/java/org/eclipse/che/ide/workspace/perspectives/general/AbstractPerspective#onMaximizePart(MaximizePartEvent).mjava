  @Override
  public void onMaximizePart(MaximizePartEvent event) {
    PartStack partStack = findPartStackByPart(event.getPart());
    if (partStack == null) {
      return;
    }

    if (partStack.getPartStackState() == PartStack.State.MAXIMIZED) {
      onRestore(partStack);
    } else {
      onMaximize(partStack);
    }
  }

