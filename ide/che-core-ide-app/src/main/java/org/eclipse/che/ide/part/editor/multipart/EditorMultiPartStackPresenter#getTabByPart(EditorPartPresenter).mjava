    @Override
    public EditorTab getTabByPart(EditorPartPresenter editorPart) {
        for (EditorPartStack editorPartStack : partStackPresenters) {
            EditorTab editorTab = editorPartStack.getTabByPart(editorPart);
            if (editorTab != null) {
                return editorTab;
            }
        }
        return null;
    }

