  /** {@inheritDoc} */
  @Override
  protected void updateProjectAction(ActionEvent e) {
    List<?> selection = projectExplorer.getSelection().getAllElements();

    e.getPresentation()
        .setEnabledAndVisible(
            !projectExplorer.isGoIntoActivated()
                && selection.size() == 1
                && isNodeSupportGoInto(selection.get(0)));
  }

