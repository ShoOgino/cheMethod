    @Override
    public void reRunProcessButtonClicked() {
        if (isFinished()) {
            commandExecutor.executeCommand(command, machine);
        } else {
            // FIXME: spi
            execAgentCommandManager.killProcess(""/*machine.getId()*/, pid)
                                   .onSuccess(() -> commandExecutor.executeCommand(command, machine));
        }
    }

