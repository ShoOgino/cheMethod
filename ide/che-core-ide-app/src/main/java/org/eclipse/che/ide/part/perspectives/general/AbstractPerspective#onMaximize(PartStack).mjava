    @Override
    public void onMaximize(PartStack partStack) {
        if (partStack == null) {
            return;
        }

        if (partStack.equals(maximizedPartStack)) {
            return;
        }

        maximizedPartStack = partStack;

        for (PartStack ps : partStacks.values()) {
            if (!ps.equals(partStack)) {
                ps.minimize();
            }
        }

        partStack.maximize();
    }

