  private void initializeJsonRpcService() {
    Log.debug(WsAgentJsonRpcInitializer.class, "Web socket agent started event caught.");

    try {
      internalInitialize();
    } catch (Exception e) {
      Log.debug(WsAgentJsonRpcInitializer.class, "Failed, will try one more time.");

      new Timer() {
        @Override
        public void run() {
          internalInitialize();
        }
      }.schedule(1_000);
    }
  }

