  /** {@inheritDoc} */
  @Override
  @NotNull
  public List<ProcessTreeNode> getChildren(ProcessTreeNode data) {
    List<ProcessTreeNode> children = new ArrayList<>();
    Collection<ProcessTreeNode> nodes = data.getChildren();

    if (nodes == null) {
      return children;
    }

    for (ProcessTreeNode node : nodes) {
      children.add(node);
    }

    return children;
  }

