    private String extractFromHtmlFormat(String text) {
        int beginIndex = -1;
        int lastIndex = -1;

        if (text.contains("<pre")) {
            beginIndex = text.indexOf(">") + 1;
            lastIndex = text.lastIndexOf("</pre");
        }
        return beginIndex != 0 && lastIndex != -1 ? text.substring(beginIndex, lastIndex) : text;
    }

