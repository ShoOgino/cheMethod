  @Override
  public void delete(final Breakpoint breakpoint) {
    readAllBreakpointMarker.then(
        onFulfilled -> {
          BreakpointStorageImpl.this.breakpoints.removeIf(
              b ->
                  b.getLineNumber() == breakpoint.getLineNumber()
                      && b.getPath().equals(breakpoint.getPath()));

          preserve();
        });
  }

