  @Inject
  private void subscribe(RequestTransmitter requestTransmitter) {
    if (isSubscribed) {
      return;
    }

    requestTransmitter
        .newRequest()
        .endpointId(WS_AGENT_ENDPOINT)
        .methodName(CLIENT_SUBSCRIBE_METHOD_NAME)
        .noParams()
        .sendAndSkipResult();

    isSubscribed = true;
  }

