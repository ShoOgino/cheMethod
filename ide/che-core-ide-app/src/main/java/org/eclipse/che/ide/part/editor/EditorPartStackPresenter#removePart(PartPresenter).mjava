  /** {@inheritDoc} */
  @Override
  public void removePart(PartPresenter part) {
    super.removePart(part);
    partsOrder.remove(part);
    activePart = partsOrder.isEmpty() ? null : partsOrder.getLast();

    if (activePart != null) {
      onRequestFocus();
    }

    if (parts.isEmpty()) {
      removeHandlers();
    }
  }

