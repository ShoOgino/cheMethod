    @Override
    public void onToggleMaximizeConsole() {
        super.onToggleMaximize();

        if (partStack != null) {
            if (partStack.getPartStackState() == PartStack.State.MAXIMIZED) {
                view.setProcessesTreeVisible(false);
            } else {
                view.setProcessesTreeVisible(true);
            }
        }
    }

