  @Override
  public Icon getIcon(String id) {
    Icon icon = icons.get(id);
    if (icon == null) {
      final String prefix = id.split("\\.")[0];
      final String defaultIconId = id.replaceFirst(prefix, "default");
      icon = icons.get(defaultIconId);
      if (icon == null) {
        icon = getGenericIcon();
      }
    }
    return icon;
  }

