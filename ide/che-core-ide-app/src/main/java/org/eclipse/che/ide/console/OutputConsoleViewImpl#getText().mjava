  @Override
  public String getText() {
    String text = "";
    NodeList<Node> nodes = consoleLines.getElement().getChildNodes();

    for (int i = 0; i < nodes.getLength(); i++) {
      Node node = nodes.getItem(i);
      Element element = node.cast();
      text += element.getInnerText() + "\r\n";
    }

    return text;
  }

