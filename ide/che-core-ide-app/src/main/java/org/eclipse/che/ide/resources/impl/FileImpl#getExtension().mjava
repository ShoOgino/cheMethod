    /** {@inheritDoc} */
    @Override
    public String getExtension() {
        final String entryName = getName();
        int lastDotIndex = entryName.lastIndexOf('.');
        if (lastDotIndex == -1) {
            return null;
        }

        if (lastDotIndex == (entryName.length() - 1)) {
            return "";
        }

        return entryName.substring(lastDotIndex + 1);
    }

