  private Comparator<ProjectImporterDescriptor> getProjectImporterComparator(
      String defaultImporterId) {
    return (o1, o2) -> {
      if (o1.getId().equals(defaultImporterId)) {
        return -1;
      }

      if (o2.getId().equals(defaultImporterId)) {
        return 1;
      }

      return 0;
    };
  }

