  /** {@inheritDoc} */
  @Override
  public boolean equals(Object o) {
    if (this == o) {
      return true;
    }

    if (!(o instanceof SyntheticNode)) {
      return false;
    }

    SyntheticNode node = (SyntheticNode) o;

    return node.getData().equals(getData()) && node.getProject().equals(getProject());
  }

