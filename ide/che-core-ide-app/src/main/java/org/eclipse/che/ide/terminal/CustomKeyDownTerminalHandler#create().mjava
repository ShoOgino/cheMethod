  public static native CustomKeyDownTerminalHandler create() /*-{
        return function(ev) {
            var C = 67;
            var V = 86;
            if (ev.ctrlKey && !(ev.shiftKey || ev.metaKey || ev.altKey)) {

                // handle Ctrl + V
                if (ev.keyCode === V) {
                    return false;
                }

                // handle Ctrl + C. Notice: scope "this" it's a terminal scope.
                if (ev.keyCode === C && this.hasSelection()) {
                    return false;
                }
            }
        }
    }-*/;

