  private void unsubscribe(String projectName) {
    String key = appContext.getWorkspaceId() + projectName;

    checkoutContextRegistry.remove(key);
    requestTransmitter
        .newRequest()
        .endpointId("ws-agent")
        .methodName("git/checkoutOutput/unsubscribe")
        .paramsAsString(key)
        .sendAndSkipResult();
  }

