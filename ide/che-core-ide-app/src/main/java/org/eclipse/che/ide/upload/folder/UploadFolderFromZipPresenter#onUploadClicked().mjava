  /** {@inheritDoc} */
  @Override
  public void onUploadClicked() {

    view.setAction(
        appContext.getWsAgentServerApiEndpoint()
            + "/project/upload/zipfolder"
            + container.getLocation()
            + "?skipFirstLevel="
            + view.isSkipRootFolderSelected()
            + "&force="
            + view.isOverwriteFileSelected());
    view.setLoaderVisibility(true);
    view.submit();
  }

