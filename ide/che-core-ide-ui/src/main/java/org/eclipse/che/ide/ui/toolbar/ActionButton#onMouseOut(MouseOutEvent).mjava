    /** Mouse Out handler. */
    @Override
    public void onMouseOut(MouseOutEvent event) {
        if (!enabled) {
            return;
        }

        if (selected) {
            panel.setStyleName(toolbarResources.toolbar().iconButtonPanelSelected());
        } else {
            panel.setStyleName(toolbarResources.toolbar().iconButtonPanel());
        }
    }

