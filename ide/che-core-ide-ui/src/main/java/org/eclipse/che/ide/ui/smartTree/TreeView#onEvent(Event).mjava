    public void onEvent(Event ce) {

        int type = ce.getTypeInt();
        switch (type) {
            case Event.ONMOUSEOVER:
                if (tree.isTrackMouseOver()) {
                    onMouseOver(ce);
                }
                break;
            case Event.ONMOUSEOUT:
                if (tree.isTrackMouseOver()) {
                    onMouseOut(ce);
                }
                break;
        }
    }

