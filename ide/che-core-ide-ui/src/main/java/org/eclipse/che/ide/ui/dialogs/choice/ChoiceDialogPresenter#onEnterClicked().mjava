    @Override
    public void onEnterClicked() {
        if (view.isFirstButtonInFocus()) {
            firstChoiceClicked();
            return;
        }

        if (view.isSecondButtonInFocus()) {
            secondChoiceClicked();
            return;
        }

        if (view.isThirdButtonInFocus()) {
            thirdChoiceClicked();
        }
    }

