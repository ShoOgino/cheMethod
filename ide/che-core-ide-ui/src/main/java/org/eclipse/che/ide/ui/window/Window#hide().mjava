  /**
   * Remove the current window from the DOM and hide it. After window hide, method {@link #onHide()}
   * is called to allow user to perform some actions after hide.
   */
  public final void hide() {
    view.detach();
    windowManager.unregister(this);

    if (keyBinding != null) {
      keyBinding.enable();
    }

    onHide();
  }

