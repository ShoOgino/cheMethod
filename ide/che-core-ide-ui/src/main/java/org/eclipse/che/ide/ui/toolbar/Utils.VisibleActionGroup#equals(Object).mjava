        @Override
        public boolean equals(Object o) {
            if (this == o) {
                return true;
            }

            if (!(o instanceof VisibleActionGroup)) {
                return false;
            }

            VisibleActionGroup other = (VisibleActionGroup)o;

            return Objects.equals(groupId, other.groupId)
                   && Objects.equals(actionList, other.actionList);
        }

