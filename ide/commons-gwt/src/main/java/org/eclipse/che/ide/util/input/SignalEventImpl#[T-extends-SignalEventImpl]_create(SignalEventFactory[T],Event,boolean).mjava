  public static <T extends SignalEventImpl> T create(
      SignalEventFactory<T> factory, Event event, boolean cancelBubbleIfNullified) {
    if (hasBeenConsumed(event)) {
      return null;
    } else {
      T signal = createInner(factory, event);
      if (cancelBubbleIfNullified && signal == null) {
        event.stopPropagation();
      }
      return signal;
    }
  }

