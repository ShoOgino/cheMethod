  /** Is this a letter/symbol that changes if the SHIFT key is pressed? */
  public static boolean needsShift(int keycode) {
    if ('A' <= keycode && keycode <= 'Z') {
      // upper case letter
      return true;
    }

    if (lowerToUpper.contains(keycode)) {
      // special character !@#$%...
      return true;
    }

    return false;
  }

