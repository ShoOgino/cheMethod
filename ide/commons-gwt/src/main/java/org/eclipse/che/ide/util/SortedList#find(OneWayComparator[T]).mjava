  /**
   * Returns the first item in the list matched by the comparator, or null if there were no matches.
   */
  public T find(OneWayComparator<T> comparator) {
    int index = findInsertionIndex(comparator);
    if (index < size()) {
      T item = get(index);
      if (comparator.compareTo(item) == 0) {
        return item;
      }
    }

    return null;
  }

