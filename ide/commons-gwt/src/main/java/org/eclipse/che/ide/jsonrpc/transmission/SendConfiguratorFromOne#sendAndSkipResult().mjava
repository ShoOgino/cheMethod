    public void sendAndSkipResult() {
        Log.debug(getClass(), "Transmitting request: " +
                              "endpoint ID: " + endpointId + ", " +
                              "method: " + method + ", " +
                              (pValue != null ? "params object class: " + pValue.getClass() + ", " : "") +
                              "params list value" + pValue);

        transmitNotification();
    }

