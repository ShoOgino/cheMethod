    /** @see java.net.URLConnection#getContentType() */
    @Override
    public String getContentType() {
        try {
            if (!connected) {
                connect();
            }
            return item.getMimeType();
        } catch (IOException e) {
            if (LOG.isDebugEnabled()) {
                LOG.debug(e.getMessage(), e);
            }
        }
        return null;
    }

