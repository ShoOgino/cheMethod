    @Override
    public boolean equals(Object o) {
        if (this == o) {
            return true;
        }
        if ((o instanceof LocalVirtualFile)) {
            LocalVirtualFile other = (LocalVirtualFile)o;
            return Objects.equals(path, other.path)
                   && Objects.equals(fileSystem, other.fileSystem);
        }
        return false;

    }

