  public FieldImpl(
      String name,
      SimpleValue value,
      String type,
      boolean isPrimitive,
      VariablePath variablePath,
      boolean isFinal,
      boolean isStatic,
      boolean isTransient,
      boolean isVolatile) {
    super(type, name, value, isPrimitive, variablePath);
    this.isFinal = isFinal;
    this.isStatic = isStatic;
    this.isTransient = isTransient;
    this.isVolatile = isVolatile;
  }

