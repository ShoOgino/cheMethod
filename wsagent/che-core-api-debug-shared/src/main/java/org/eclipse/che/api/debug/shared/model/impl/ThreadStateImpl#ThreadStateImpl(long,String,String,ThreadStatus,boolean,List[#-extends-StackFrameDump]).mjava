  public ThreadStateImpl(
      long id,
      String name,
      String groupName,
      ThreadStatus status,
      boolean isSuspended,
      List<? extends StackFrameDump> frames) {
    this.name = name;
    this.groupName = groupName;
    this.status = status;
    this.isSuspended = isSuspended;
    this.frames = frames != null ? frames : new ArrayList<>();
    this.id = id;
  }

