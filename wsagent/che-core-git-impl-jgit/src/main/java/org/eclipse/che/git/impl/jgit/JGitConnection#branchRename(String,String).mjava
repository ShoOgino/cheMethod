    @Override
    public void branchRename(String oldName, String newName) throws GitException {
        try {
            getGit().branchRename()
                    .setOldName(oldName)
                    .setNewName(newName)
                    .call();
        } catch (GitAPIException exception) {
            throw new GitException(exception.getMessage(), exception);
        }
    }

