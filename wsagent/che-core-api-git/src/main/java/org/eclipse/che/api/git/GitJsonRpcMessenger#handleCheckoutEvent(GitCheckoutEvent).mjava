  private void handleCheckoutEvent(GitCheckoutEvent event) {
    endpointIdsWithWorkspaceIdAndProjectName
        .entrySet()
        .stream()
        .filter(it -> it.getValue().contains(event.getWorkspaceId() + event.getProjectName()))
        .map(Entry::getKey)
        .forEach(
            it ->
                transmitter
                    .newRequest()
                    .endpointId(it)
                    .methodName("git/checkoutOutput")
                    .paramsAsDto(event)
                    .sendAndSkipResult());
  }

