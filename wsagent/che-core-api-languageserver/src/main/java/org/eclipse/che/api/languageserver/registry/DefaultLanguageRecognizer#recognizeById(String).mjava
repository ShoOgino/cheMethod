  @Override
  public LanguageDescription recognizeById(String id) {
    for (LanguageDescription language : languages) {
      if (language.getLanguageId().equals(id)) {
        return language;
      }
    }

    for (LanguageDescription language : defaultLanguages.getAll()) {
      if (language.getLanguageId().equals(id)) {
        return language;
      }
    }

    return UNIDENTIFIED;
  }

