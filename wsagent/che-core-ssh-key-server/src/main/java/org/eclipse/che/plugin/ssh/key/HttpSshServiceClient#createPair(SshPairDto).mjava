  @Override
  public void createPair(SshPairDto sshPair) throws ServerException {
    try {
      requestFactory
          .fromUrl(sshUrl)
          .usePostMethod()
          .setBody(sshPair)
          .request()
          .asDto(SshPairDto.class);
    } catch (IOException | ApiException e) {
      throw new ServerException(e);
    }
  }

