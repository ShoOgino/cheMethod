  @Override
  public Optional<ProjectConfig> get(String wsPath) throws ServerException {
    return workspaceDto()
        .getConfig()
        .getProjects()
        .stream()
        .filter(ProjectConfig.class::isInstance)
        .map(it -> (ProjectConfig) it)
        .filter(it -> it.getPath().equals(wsPath))
        .findAny();
  }

