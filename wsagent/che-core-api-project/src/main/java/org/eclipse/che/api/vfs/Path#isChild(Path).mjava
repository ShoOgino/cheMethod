  public boolean isChild(Path parent) {
    if (parent.elements.length >= this.elements.length) {
      return false;
    }
    for (int i = 0, parentLength = parent.elements.length; i < parentLength; i++) {
      if (!parent.elements[i].equals(this.elements[i])) {
        return false;
      }
    }
    return true;
  }

