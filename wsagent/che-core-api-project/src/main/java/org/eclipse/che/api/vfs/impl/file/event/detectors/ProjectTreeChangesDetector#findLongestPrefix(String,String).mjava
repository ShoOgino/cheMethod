    public static String findLongestPrefix(String s1, String s2) {
        if (s1 == null) {
            return s2;
        }

        for (int i = min(s1.length(), s2.length()); ; i--) {
            if (s2.startsWith(s1.substring(0, i))) {
                return s1.substring(0, i);
            }
        }
    }

