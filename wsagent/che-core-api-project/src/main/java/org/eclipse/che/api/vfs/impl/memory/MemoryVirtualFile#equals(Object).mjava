  @Override
  public boolean equals(Object o) {
    if (this == o) {
      return true;
    }
    if (!(o instanceof MemoryVirtualFile)) {
      return false;
    }
    MemoryVirtualFile other = (MemoryVirtualFile) o;
    return Objects.equals(fileSystem, other.fileSystem)
        && Objects.equals(getPath(), other.getPath());
  }

