    public OffsetData(
        String phrase,
        int startOffset,
        int endOffset,
        int docId,
        float score,
        int lineNum,
        String line) {
      this.phrase = phrase;
      this.startOffset = startOffset;
      this.endOffset = endOffset;
      this.docId = docId;
      this.score = score;
      this.lineNum = lineNum;
      this.line = line;
    }

