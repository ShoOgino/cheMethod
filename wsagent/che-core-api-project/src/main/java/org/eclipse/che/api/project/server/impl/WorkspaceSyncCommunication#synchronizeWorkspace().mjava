  /** Sends workspace updating event */
  void synchronizeWorkspace() {
    clientSubscriptionHandler
        .getEndpointIds()
        .forEach(
            it ->
                transmitter
                    .newRequest()
                    .endpointId(it)
                    .methodName(WORKSPACE_SYNCHRONIZE_METHOD_NAME)
                    .noParams()
                    .sendAndSkipResult());
  }

