  private void addVfsFilter(Multibinder<PathMatcher> excludeMatcher, String filter) {
    excludeMatcher
        .addBinding()
        .toInstance(
            path -> {
              for (Path pathElement : path) {
                if (pathElement == null || filter.equals(pathElement.toString())) {
                  return true;
                }
              }
              return false;
            });
  }

