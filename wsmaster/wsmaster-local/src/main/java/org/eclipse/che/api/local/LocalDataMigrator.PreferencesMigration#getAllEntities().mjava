        @Override
        public List<Pair<String, Map<String, String>>> getAllEntities() throws Exception {
            return storage.loadMap(new TypeToken<Map<String, Map<String, String>>>() {})
                          .entrySet()
                          .stream()
                          .map(e -> Pair.of(e.getKey(), e.getValue()))
                          .collect(Collectors.toList());
        }

