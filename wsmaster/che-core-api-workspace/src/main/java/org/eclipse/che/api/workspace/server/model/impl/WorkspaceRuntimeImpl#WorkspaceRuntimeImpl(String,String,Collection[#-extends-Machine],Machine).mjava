  public WorkspaceRuntimeImpl(
      String activeEnv,
      String rootFolder,
      Collection<? extends Machine> machines,
      Machine devMachine) {
    this.activeEnv = activeEnv;
    this.rootFolder = rootFolder;
    if (devMachine != null) {
      this.devMachine = new MachineImpl(devMachine);
    }
    if (machines != null) {
      this.machines = machines.stream().map(MachineImpl::new).collect(toList());
    }
  }

