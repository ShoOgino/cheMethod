  @Inject
  public WebsocketTerminalFilesPathProvider(ConfigurationProperties configurationProperties) {
    archivesPaths =
        configurationProperties
            .getProperties(CONFIGURATION_PREFIX_PATTERN)
            .entrySet()
            .stream()
            .collect(
                toMap(
                    entry -> entry.getKey().replaceFirst(CONFIGURATION_PREFIX, ""),
                    Map.Entry::getValue));
  }

