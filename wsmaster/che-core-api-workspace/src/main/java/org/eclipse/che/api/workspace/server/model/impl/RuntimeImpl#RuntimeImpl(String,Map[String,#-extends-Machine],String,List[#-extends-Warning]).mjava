  public RuntimeImpl(
      String activeEnv,
      Map<String, ? extends Machine> machines,
      String owner,
      List<? extends Warning> warnings) {
    this.activeEnv = activeEnv;
    this.machines = machines;
    this.owner = owner;
    this.warnings = warnings.stream().map(WarningImpl::new).collect(Collectors.toList());
  }

