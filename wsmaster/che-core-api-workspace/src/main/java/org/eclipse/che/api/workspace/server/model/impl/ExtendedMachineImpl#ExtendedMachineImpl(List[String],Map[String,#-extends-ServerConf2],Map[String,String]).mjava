  public ExtendedMachineImpl(
      List<String> agents,
      Map<String, ? extends ServerConf2> servers,
      Map<String, String> attributes) {
    if (agents != null) {
      this.agents = new ArrayList<>(agents);
    }
    if (servers != null) {
      this.servers =
          servers
              .entrySet()
              .stream()
              .collect(
                  Collectors.toMap(
                      Map.Entry::getKey, entry -> new ServerConf2Impl(entry.getValue())));
    }
    if (attributes != null) {
      this.attributes = new HashMap<>(attributes);
    }
  }

