  @Override
  public boolean equals(Object o) {
    if (this == o) {
      return true;
    }
    if (!(o instanceof VolumeImpl)) {
      return false;
    }
    VolumeImpl volume = (VolumeImpl) o;
    return Objects.equals(getName(), volume.getName())
        && Objects.equals(getContainerPath(), volume.getContainerPath());
  }

