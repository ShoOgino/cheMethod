    private static MachineConfigImpl createConfig(boolean isDev) {
        return MachineConfigImpl.builder()
                                .setDev(isDev)
                                .setType("docker")
                                .setLimits(new LimitsImpl(1024))
                                .setSource(new MachineSourceImpl("git", "location"))
                                .setName("dev-machine")
                                .build();
    }

