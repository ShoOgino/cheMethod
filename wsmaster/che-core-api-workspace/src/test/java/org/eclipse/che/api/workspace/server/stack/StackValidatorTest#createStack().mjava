  private static StackDto createStack() {
    return newDto(StackDto.class)
        .withId("stack123")
        .withName("Name")
        .withDescription("Description")
        .withScope("general")
        .withCreator("user123")
        .withTags(new ArrayList<>(Collections.singletonList("latest")))
