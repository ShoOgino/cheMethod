  public AbstractMachineProcess(
      String name,
      String commandLine,
      String type,
      Map<String, String> attributes,
      int pid,
      String outputChannel) {
    this.name = name;
    this.commandLine = commandLine;
    this.type = type;
    this.attributes = attributes;
    this.outputChannel = outputChannel;
    this.pid = pid;
  }

