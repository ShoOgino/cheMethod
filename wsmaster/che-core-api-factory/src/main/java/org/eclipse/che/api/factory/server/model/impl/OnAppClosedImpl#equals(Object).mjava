    @Override
    public boolean equals(Object obj) {
        if (this == obj) {
            return true;
        }
        if (!(obj instanceof OnAppClosedImpl)) {
            return false;
        }
        final OnAppClosedImpl that = (OnAppClosedImpl)obj;
        return Objects.equals(id, that.id)
               && getActions().equals(that.getActions());
    }

