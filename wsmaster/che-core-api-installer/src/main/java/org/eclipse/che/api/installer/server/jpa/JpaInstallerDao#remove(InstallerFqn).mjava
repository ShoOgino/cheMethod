    @Override
    public void remove(InstallerFqn fqn) throws InstallerException {
        requireNonNull(fqn, "Required non-null fqn");
        try {
            doRemove(fqn);
        } catch (NoResultException e) {
        } catch (RuntimeException x) {
            throw new InstallerException(x.getMessage(), x);
        }
    }

