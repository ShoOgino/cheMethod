  protected void onSuccessRequest(JsArrayMixed result) {
    sshKeyManager
        .isSshKeyAvailable(GITHUB_HOST)
        .then(
            isSshAvailable -> {
              sshKeyGenerationInfo.setSshAvailable(isSshAvailable);
              onListLoaded(toOrgList(result), toRepoList(result));
              showProcessing(false);
            });
  }

