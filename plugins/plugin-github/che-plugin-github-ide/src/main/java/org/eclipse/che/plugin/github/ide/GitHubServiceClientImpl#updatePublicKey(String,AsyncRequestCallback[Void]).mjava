  /** {@inheritDoc} */
  @Override
  public void updatePublicKey(String oauthToken, @NotNull AsyncRequestCallback<Void> callback) {
    String url = baseUrl() + SSH_GEN;
    asyncRequestFactory
        .createPostRequest(url, null)
        .header(TOKEN_HEADER_NAME, oauthToken)
        .loader(loader)
        .send(callback);
  }

