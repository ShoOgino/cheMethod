    /**
     * Provides the location to dockerfile
     *
     * @return location of dockerfile in a repository
     */
    protected String dockerFileLocation() {
        return new StringJoiner("/").add("https://raw.githubusercontent.com")
                                    .add(username)
                                    .add(repository)
                                    .add(branch)
                                    .add(dockerfileFilename)
                                    .toString();
    }

