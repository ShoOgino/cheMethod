  public static String getBlobUrl(
      String rootGitRepoUrl, String ref, String path, int lineStart, int lineEnd) {
    return rootGitRepoUrl
        + "/blob/"
        + ref
        + "/"
        + path
        + (lineStart > 0 ? "#L" + lineStart : "")
        + (lineEnd > 0 ? "-L" + lineEnd : "");
  }

