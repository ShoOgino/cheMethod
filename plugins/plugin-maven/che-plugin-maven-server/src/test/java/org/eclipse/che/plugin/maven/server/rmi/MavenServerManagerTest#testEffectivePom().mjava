  @Test
  public void testEffectivePom() throws Exception {
    MavenServerWrapper mavenServer = manager.createMavenServer();
    String effectivePom =
        mavenServer.getEffectivePom(
            new File(MavenServerManagerTest.class.getResource("/EffectivePom/pom.xml").getFile()),
            Collections.emptyList(),
            Collections.emptyList());
    assertThat(effectivePom)
        .isNotNull()
        .isNotEmpty()
        .contains("<!-- Effective POM for project")
        .contains("'org.eclipse.che.parent:maven-parent-pom:pom:4.0.0-M6-SNAPSHOT'");
  }

