    @Override
    public void print(int level, String message, Throwable throwable) throws RemoteException {
        switch (level) {
            case LEVEL_DEBUG:
                LOG.debug(message, throwable);
                break;
            case LEVEL_ERROR:
            case LEVEL_FATAL:
                LOG.error(message, throwable);
                break;
            case LEVEL_INFO:
                LOG.info(message, throwable);
                break;
            case LEVEL_WARN:
                LOG.warn(message, throwable);
                break;

        }
    }

