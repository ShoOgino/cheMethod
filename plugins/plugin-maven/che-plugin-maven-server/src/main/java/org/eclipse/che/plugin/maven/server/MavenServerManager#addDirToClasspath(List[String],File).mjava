  private static void addDirToClasspath(List<String> classPath, File dir) {
    File[] jars =
        dir.listFiles(
            (dir1, name) -> {
              return name.endsWith(".jar");
            });

    if (jars == null) {
      return;
    }

    for (File jar : jars) {
      classPath.add(jar.getAbsolutePath());
    }
  }

