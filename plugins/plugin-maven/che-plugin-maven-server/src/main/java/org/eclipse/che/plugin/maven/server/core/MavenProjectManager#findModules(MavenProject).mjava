  public List<MavenProject> findModules(MavenProject parent) {
    readLock.lock();
    try {
      List<MavenProject> modules = parentToModulesMap.get(parent);
      if (modules == null) {
        modules = Collections.emptyList();
      }
      return new ArrayList<>(modules);
    } finally {
      readLock.unlock();
    }
  }

