    private void print(int level, String message, Throwable t) {
        if (level < logLevel) {
            return;
        }

        if (terminal != null) {
            if (!message.endsWith("\n")) {
                message += "\n";
            }
            try {
                terminal.print(level, message, t);
            } catch (RemoteException ignore) {
            }
        }
    }

