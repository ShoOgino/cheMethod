    public static boolean isElementEquals(Element e1, Element e2) {
        if (e1 == null && e2 == null) {
            return true;
        }

        if (e1 == null || e2 == null) {
            return false;
        }

        return Objects.equals(e1.getName(), e2.getName()) && isAttributesEqual(e1.getAttributes(), e2.getAttributes());
    }

