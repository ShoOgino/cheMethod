  private static boolean isAttributesEqual(List l1, List l2) {
    if (l1.size() != l2.size()) {
      return false;
    }
    for (int i = 0; i < l1.size(); i++) {
      if (!isAttributeEqual((Attribute) l1.get(i), (Attribute) l2.get(i))) {
        return false;
      }
    }
    return true;
  }

