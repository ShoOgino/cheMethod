  /**
   * Returns model identifier
   *
   * @return the model id as <code>groupId:artifactId:packaging:version</code>
   */
  public String getId() {
    return (version == null ? "[inherited]" : groupId)
        + ':'
        + artifactId
        + ':'
        + packaging
        + ':'
        + (version == null ? "[inherited]" : version);
  }

