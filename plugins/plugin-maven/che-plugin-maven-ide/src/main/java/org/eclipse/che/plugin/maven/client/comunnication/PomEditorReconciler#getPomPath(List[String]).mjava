  private Set<String> getPomPath(List<String> updatedProjects) {
    Set<String> result = new HashSet<>();
    for (String projectPath : updatedProjects) {
      String pomPath =
          projectPath.endsWith("/") ? projectPath + "pom.xml" : projectPath + "/pom.xml";
      result.add(pomPath);
    }

    return result;
  }

