  private void generateKey(String host) {
    service
        .generatePair(SSH_SERVICE, host)
        .then(
            pair -> {
              downloadPrivateKey(pair.getPrivateKey());
              refreshKeys();
            })
        .catchError(
            arg -> {
              notificationManager.notify(
                  constant.failedToGenerateSshKey(), arg.getMessage(), FAIL, FLOAT_MODE);
            });
  }

