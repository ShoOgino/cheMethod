    @Override
    public void setTerminalSize(int x, int y) {
        if (!connected) {
            return;
        }

        terminal.resize(x, y);
        terminal.focus();
        Jso jso = Jso.create();
        JsArrayInteger arr = Jso.createArray().cast();
        arr.set(0, x);
        arr.set(1, y);
        jso.addField("type", "resize");
        jso.addField("data", arr);
        socket.send(jso.serialize());
    }

