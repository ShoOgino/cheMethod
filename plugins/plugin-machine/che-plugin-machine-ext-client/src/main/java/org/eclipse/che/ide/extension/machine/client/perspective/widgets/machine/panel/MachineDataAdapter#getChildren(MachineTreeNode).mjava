    /** {@inheritDoc} */
    @Override
    @NotNull
    public List<MachineTreeNode> getChildren(MachineTreeNode data) {
        List<MachineTreeNode> children = new ArrayList<>();

        Collection<MachineTreeNode> nodes = data.getChildren();

        if (nodes == null) {
            return children;
        }

        for (MachineTreeNode node : nodes) {
            children.add(node);
        }

        return children;
    }

