    private void resizeTerminal() {
        TerminalGeometryJso geometryJso = terminal.proposeGeometry();
        int x = geometryJso.getCols();
        int y = geometryJso.getRows();
        if (x <= 0 || y <= 0) {
            resizeTimer.cancel();
            resizeTimer.schedule(500);
            return;
        }

        delegate.setTerminalSize(x, y);
    }

