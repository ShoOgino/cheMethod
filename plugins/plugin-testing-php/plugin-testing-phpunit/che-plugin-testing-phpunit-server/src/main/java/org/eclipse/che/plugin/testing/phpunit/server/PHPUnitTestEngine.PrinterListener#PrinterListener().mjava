    public PrinterListener() {
      threadExecutor =
          Executors.newSingleThreadExecutor(
              new ThreadFactory() {
                @Override
                public Thread newThread(Runnable r) {
                  final Thread thread = new Thread(r, "PHPUnitPrinterListener");
                  thread.setDaemon(true);
                  return thread;
                }
              });
    }

