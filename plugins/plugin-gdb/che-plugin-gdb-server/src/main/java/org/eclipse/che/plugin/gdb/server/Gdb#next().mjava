    /**
     * `next` command.
     */
    @Nullable
    public GdbInfoLine next() throws IOException, InterruptedException, DebuggerException {
        sendCommand("next");

        GdbInfoProgram gdbInfoProgram = infoProgram();
        if (gdbInfoProgram.getStoppedAddress() == null) {
            return null;
        }

        return infoLine();
    }

