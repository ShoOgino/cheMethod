    GdbDebugger(String host, int port, String name, String version, String id, String file, Gdb gdb) {
        this.host = host;
        this.port = port;
        this.name = name;
        this.version = version;
        this.id = id;
        this.file = file;
        this.gdb = gdb;

        instances.put(id, this);
    }

