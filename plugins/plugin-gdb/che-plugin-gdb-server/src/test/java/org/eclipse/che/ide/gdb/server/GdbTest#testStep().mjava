    @Test
    public void testStep() throws Exception {
        gdb.file(file);
        gdb.breakpoint(7);
        gdb.run();

        GdbInfoLine gdbInfoLine = gdb.step();
        assertNotNull(gdbInfoLine.getLocation());

        gdbInfoLine = gdb.step();
        assertNotNull(gdbInfoLine.getLocation());
    }

