        @Override
        public Violation validate(String value) {
            if (!hostNamePattern.test(value)) {
                return new Violation() {
                    @Override
                    public String getMessage() {
                        return constant.invalidHostName();
                    }

                    @Override
                    public String getCorrectedValue() {
                        return null;
                    }
                };
            }
            return null;
        }

