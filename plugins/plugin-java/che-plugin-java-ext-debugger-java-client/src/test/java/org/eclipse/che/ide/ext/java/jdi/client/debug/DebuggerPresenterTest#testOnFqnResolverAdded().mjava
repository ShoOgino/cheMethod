    @Test
    public void testOnFqnResolverAdded() {
        FqnResolver fqnResolver = mock(FqnResolver.class);

        List<Breakpoint> breakpoints = new ArrayList();
        doReturn(breakpoints).when(breakpointManager).getBreakpointList();

        presenter.onFqnResolverAdded(fqnResolver);
        verify(breakpointManager).getBreakpointList();
        verify(view).setBreakpoints(any());
    }

