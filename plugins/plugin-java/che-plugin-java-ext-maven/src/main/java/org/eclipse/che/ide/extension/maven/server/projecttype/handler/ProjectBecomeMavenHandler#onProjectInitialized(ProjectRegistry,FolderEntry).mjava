    @Override
    public void onProjectInitialized(ProjectRegistry projectRegistry, FolderEntry projectFolder) throws ServerException,
                                                                                                        ForbiddenException,
                                                                                                        ConflictException,
                                                                                                        NotFoundException {
        try {
            MavenProjectResolver.resolve(projectFolder, projectRegistry);
        } catch (IOException e) {
            throw new ServerException(e);
        }
    }

