    @Override
    public Value evaluate(Evaluator ev) {
        try {
            if (nodes == null) {
                parse();
            } else {
                nodes.reset();
            }
            JavaTreeParser walker = new JavaTreeParser(nodes, ev);
            return walker.evaluate();
        } catch (RecognitionException e) {
            throw new ExpressionException(e.getMessage(), e);
        }
    }

