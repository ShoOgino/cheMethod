    @Override
    public JdiField getFieldByName(String name) throws DebuggerException {
        if (name == null) {
            throw new IllegalArgumentException("Field name may not be null. ");
        }
        for (JdiField f : getFields()) {
            if (name.equals(f.getName())) {
                return f;
            }
        }
        return null;
    }

