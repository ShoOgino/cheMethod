  private String getJavaType(IType type) throws JavaModelException {
    if (type.isAnnotation()) {
      return "@interface";
    }
    if (type.isClass()) {
      return "class";
    }

    if (type.isInterface()) {
      return "interface";
    }

    if (type.isEnum()) {
      return "enum";
    }

    return "can't determine type";
  }

