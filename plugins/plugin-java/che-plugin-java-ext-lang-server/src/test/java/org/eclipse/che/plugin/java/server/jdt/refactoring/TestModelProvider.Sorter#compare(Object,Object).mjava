    public int compare(Object o1, Object o2) {
      IResourceDelta d1 = (IResourceDelta) o1;
      IResourceDelta d2 = (IResourceDelta) o2;
      return d1.getResource()
          .getFullPath()
          .toPortableString()
          .compareTo(d2.getResource().getFullPath().toPortableString());
    }

