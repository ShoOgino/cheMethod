  public String getTypeString() {
    switch (getType()) {
      case FILE:
        return "L"; // $NON-NLS-1$
      case FOLDER:
        return "F"; // $NON-NLS-1$
      case PROJECT:
        return "P"; // $NON-NLS-1$
      case ROOT:
        return "R"; // $NON-NLS-1$
    }
    return ""; // $NON-NLS-1$
  }

