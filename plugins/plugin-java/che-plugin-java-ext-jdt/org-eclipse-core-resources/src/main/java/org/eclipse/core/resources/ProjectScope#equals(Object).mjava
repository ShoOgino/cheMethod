    /* (non-Javadoc)
	 * @see java.lang.Object#equals(java.lang.Object)
	 */
    @Override
    public boolean equals(Object obj) {
        if (this == obj)
            return true;
        if (!super.equals(obj))
            return false;
        if (!(obj instanceof ProjectScope))
            return false;
        ProjectScope other = (ProjectScope)obj;
        return context.equals(other.context);
    }

