  /*
   * (non-Javadoc)
   *
   * @see org.eclipse.ui.ide.undo.ResourceDescription#verifyExistence(boolean)
   */
  public boolean verifyExistence(boolean checkMembers) {
    IContainer p = parent;
    if (p == null) {
      p = getWorkspace().getRoot();
    }
    IResource handle = p.findMember(getName());
    return handle != null;
  }

