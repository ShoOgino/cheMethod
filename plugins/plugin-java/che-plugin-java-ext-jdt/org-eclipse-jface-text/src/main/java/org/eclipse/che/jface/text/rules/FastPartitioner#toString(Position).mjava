  /**
   * Pretty print a <code>Position</code>.
   *
   * @param position the position to format
   * @return a formatted string
   */
  private String toString(Position position) {
    return "P["
        + position.getOffset()
        + "+"
        + position.getLength()
        + "]"; //$NON-NLS-1$ //$NON-NLS-2$ //$NON-NLS-3$
  }

