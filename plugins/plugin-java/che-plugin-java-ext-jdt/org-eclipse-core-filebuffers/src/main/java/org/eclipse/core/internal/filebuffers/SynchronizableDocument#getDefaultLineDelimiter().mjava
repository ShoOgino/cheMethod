	/*
	 * @see org.eclipse.jface.text.AbstractDocument#getDefaultLineDelimiter()
	 *
	 * @since 3.5
	 */
	public String getDefaultLineDelimiter() {
		Object lockObject= getLockObject();
		if (lockObject == null) {
			return super.getDefaultLineDelimiter();
		}
		synchronized (lockObject) {
			return super.getDefaultLineDelimiter();
		}
	}

