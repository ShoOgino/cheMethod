  /*
   * @see org.eclipse.jface.text.IDocumentExtension4#getModificationStamp()
   * @since 3.1
   */
  public long getModificationStamp() {
    Object lockObject = getLockObject();
    if (lockObject == null) {
      return super.getModificationStamp();
    }
    synchronized (lockObject) {
      return super.getModificationStamp();
    }
  }

