  @Override
  protected boolean checkTypeArgument(TType rhs) {
    switch (rhs.getKind()) {
      case ARRAY_TYPE:
      case STANDARD_TYPE:
      case PARAMETERIZED_TYPE:
      case RAW_TYPE:
      case UNBOUND_WILDCARD_TYPE:
      case EXTENDS_WILDCARD_TYPE:
      case SUPER_WILDCARD_TYPE:
      case TYPE_VARIABLE:
      case CAPTURE_TYPE:
        return true;
      default:
        return false;
    }
  }

