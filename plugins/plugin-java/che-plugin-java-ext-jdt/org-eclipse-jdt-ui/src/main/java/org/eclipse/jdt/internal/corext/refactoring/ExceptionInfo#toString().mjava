  @Override
  public String toString() {
    StringBuffer result = new StringBuffer();
    switch (fKind) {
      case OLD:
        result.append("OLD: ");
        break; //$NON-NLS-1$
      case ADDED:
        result.append("ADDED: ");
        break; //$NON-NLS-1$
      case DELETED:
        result.append("DELETED: ");
        break; //$NON-NLS-1$
    }
    if (fElement == null) result.append("null"); //$NON-NLS-1$
    else result.append(fElement.toString());
    return result.toString();
  }

