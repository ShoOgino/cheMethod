  private ParameterInfo(
      IVariableBinding binding, ITypeBinding typeBinding, String type, String name, int index) {
    fOldBinding = binding;
    fOldTypeBinding = typeBinding;
    fNewTypeBinding = typeBinding;
    fOldTypeName = type;
    fNewTypeName = type;
    fOldName = name;
    fNewName = name;
    fOldIndex = index;
    fDefaultValue = ""; //$NON-NLS-1$
    fIsDeleted = false;
  }

