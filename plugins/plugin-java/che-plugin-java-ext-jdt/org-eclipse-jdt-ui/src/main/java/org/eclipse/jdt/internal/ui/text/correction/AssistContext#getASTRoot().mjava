  public CompilationUnit getASTRoot() {
    if (fASTRoot == null) {
      fASTRoot = SharedASTProvider.getAST(fCompilationUnit, fWaitFlag, null);
      if (fASTRoot == null) {
        // see bug 63554
        fASTRoot = ASTResolving.createQuickFixAST(fCompilationUnit, null);
      }
    }
    return fASTRoot;
  }

