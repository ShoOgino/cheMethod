  public static boolean isRawTypeReferenceProblem(int id) {
    switch (id) {
      case IProblem.UnsafeTypeConversion:
      case IProblem.UnsafeElementTypeConversion:
      case IProblem.RawTypeReference:
      case IProblem.UnsafeRawMethodInvocation:
        return true;
      default:
        return false;
    }
  }

