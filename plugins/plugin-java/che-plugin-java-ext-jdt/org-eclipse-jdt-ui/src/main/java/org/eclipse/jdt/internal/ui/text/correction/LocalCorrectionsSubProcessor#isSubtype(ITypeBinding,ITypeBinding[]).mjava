  private static boolean isSubtype(ITypeBinding curr, ITypeBinding[] addedExceptions) {
    while (curr != null) {
      for (int i = 0; i < addedExceptions.length; i++) {
        if (curr == addedExceptions[i]) {
          return true;
        }
      }
      curr = curr.getSuperclass();
    }
    return false;
  }

