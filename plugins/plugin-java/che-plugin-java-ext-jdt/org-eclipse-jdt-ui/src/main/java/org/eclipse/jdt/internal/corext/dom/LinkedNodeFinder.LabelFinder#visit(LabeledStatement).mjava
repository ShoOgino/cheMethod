    @Override
    public boolean visit(LabeledStatement node) {
      if (fDefiningLabel == null) {
        SimpleName label = node.getLabel();
        if (fLabel == label || isSameLabel(label) && ASTNodes.isParent(fLabel, node)) {
          fDefiningLabel = node;
          fResult.add(label);
        }
      }
      node.getBody().accept(this);
      return false;
    }

