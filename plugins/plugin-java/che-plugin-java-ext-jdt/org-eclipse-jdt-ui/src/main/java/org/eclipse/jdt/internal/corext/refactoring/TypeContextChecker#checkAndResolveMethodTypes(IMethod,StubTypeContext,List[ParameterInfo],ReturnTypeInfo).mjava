  public static RefactoringStatus[] checkAndResolveMethodTypes(
      IMethod method,
      StubTypeContext stubTypeContext,
      List<ParameterInfo> parameterInfos,
      ReturnTypeInfo returnTypeInfo)
      throws CoreException {
