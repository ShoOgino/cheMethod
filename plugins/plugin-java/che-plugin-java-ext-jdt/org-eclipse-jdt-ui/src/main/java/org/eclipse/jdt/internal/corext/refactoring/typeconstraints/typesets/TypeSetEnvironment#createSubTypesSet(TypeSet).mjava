  public SubTypesSet createSubTypesSet(TypeSet superTypes) {
    if (fSubTypesSets.containsKey(superTypes)) {
      fgCommonExprHits++;
      return fSubTypesSets.get(superTypes);
    } else {
      SubTypesSet s = new SubTypesSet(superTypes);

      fgCommonExprMisses++;
      fSubTypesSets.put(superTypes, s);
      return s;
    }
  }

