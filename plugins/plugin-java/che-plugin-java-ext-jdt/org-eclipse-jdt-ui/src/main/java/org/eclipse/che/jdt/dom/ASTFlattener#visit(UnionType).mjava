  /*
   * @see ASTVisitor#visit(UnionType)
   */
  @Override
  public boolean visit(UnionType node) {
    for (Iterator<Type> it = node.types().iterator(); it.hasNext(); ) {
      Type t = it.next();
      t.accept(this);
      if (it.hasNext()) {
        this.fBuffer.append("|"); //$NON-NLS-1$
      }
    }
    return false;
  }

