  /*
   * @see ASTVisitor#visit(ParameterizedType)
   * @since 3.0
   */
  @Override
  public boolean visit(ParameterizedType node) {
    node.getType().accept(this);
    this.fBuffer.append("<"); // $NON-NLS-1$
    for (Iterator<Type> it = node.typeArguments().iterator(); it.hasNext(); ) {
      Type t = it.next();
      t.accept(this);
      if (it.hasNext()) {
        this.fBuffer.append(","); // $NON-NLS-1$
      }
    }
    this.fBuffer.append(">"); // $NON-NLS-1$
    return false;
  }

