  /** {@inheritDoc} */
  @Override
  protected Change createUndoChange(Change[] childUndos) {
    DynamicValidationStateChange result = new DynamicValidationStateChange(getName(), true);
    for (int i = 0; i < childUndos.length; i++) {
      result.add(childUndos[i]);
    }
    return result;
  }

