	/**
	 * {@inheritDoc}
	 */
	@Override
	public String getAdditionalProposalInfo() {
		StringBuffer sb = new StringBuffer();
		for (int i = 0; i < fOperations.length; i++) {
			CompilationUnitRewriteOperation operation = fOperations[i];
			String info = operation.getAdditionalInfo();
			if (info != null)
				sb.append(info);
		}

		if (sb.length() == 0)
			return null;

		return sb.toString();
	}

