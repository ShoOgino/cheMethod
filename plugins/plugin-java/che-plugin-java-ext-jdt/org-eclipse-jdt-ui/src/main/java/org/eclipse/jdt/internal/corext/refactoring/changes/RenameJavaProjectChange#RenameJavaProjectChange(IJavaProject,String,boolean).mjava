  public RenameJavaProjectChange(IJavaProject project, String newName, boolean updateReferences) {
    this(
        project.getPath(),
        project.getElementName(),
        newName,
        IResource.NULL_STAMP,
        updateReferences);
    Assert.isTrue(!project.isReadOnly(), "should not be read only"); //$NON-NLS-1$
  }

