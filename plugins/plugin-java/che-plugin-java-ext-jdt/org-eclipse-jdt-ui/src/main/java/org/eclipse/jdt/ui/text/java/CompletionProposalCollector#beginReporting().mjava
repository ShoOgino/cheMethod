  /**
   * {@inheritDoc}
   *
   * <p>Subclasses may extend, but must call the super implementation.
   */
  @Override
  public void beginReporting() {
    if (DEBUG) {
      fStartTime = System.currentTimeMillis();
      fUITime = 0;
    }

    fLastProblem = null;
    fJavaProposals.clear();
    fKeywords.clear();
    fSuggestedMethodNames.clear();
  }

