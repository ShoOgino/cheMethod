  private SimpleSet getKnownPackages() {
    SimpleSet packageNames = knownPackageNames;
    if (packageNames == null) {
      synchronized (this) {
        packageNames = knownPackageNames;
        if (packageNames == null) {
          packageNames = readPackages();
          knownPackageNames = packageNames;
        }
      }
    }
    return packageNames;
  }

