  /*
   * @see ASTVisitor#visit(Block)
   */
  @Override
  public boolean visit(Block node) {
    this.fBuffer.append("{"); //$NON-NLS-1$
    for (Iterator<Statement> it = node.statements().iterator(); it.hasNext(); ) {
      Statement s = it.next();
      s.accept(this);
    }
    this.fBuffer.append("}"); //$NON-NLS-1$
    return false;
  }

