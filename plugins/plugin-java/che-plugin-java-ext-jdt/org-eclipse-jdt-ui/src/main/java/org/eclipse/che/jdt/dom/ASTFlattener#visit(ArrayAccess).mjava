    /*
     * @see ASTVisitor#visit(ArrayAccess)
     */
    @Override
    public boolean visit(ArrayAccess node) {
        node.getArray().accept(this);
        this.fBuffer.append("[");//$NON-NLS-1$
        node.getIndex().accept(this);
        this.fBuffer.append("]");//$NON-NLS-1$
        return false;
    }

