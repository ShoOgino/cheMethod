    /*
     * @see ASTVisitor#visit(CastExpression)
     */
    @Override
    public boolean visit(CastExpression node) {
        this.fBuffer.append("(");//$NON-NLS-1$
        node.getType().accept(this);
        this.fBuffer.append(")");//$NON-NLS-1$
        node.getExpression().accept(this);
        return false;
    }

