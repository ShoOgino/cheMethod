  @Override
  public TType[] getSubTypes() {
    TType[] subTypes = fElementType.getSubTypes();
    TType[] result = new TType[subTypes.length];
    for (int i = 0; i < subTypes.length; i++) {
      result[i] = getEnvironment().createArrayType(subTypes[i], fDimensions);
    }
    return result;
  }

