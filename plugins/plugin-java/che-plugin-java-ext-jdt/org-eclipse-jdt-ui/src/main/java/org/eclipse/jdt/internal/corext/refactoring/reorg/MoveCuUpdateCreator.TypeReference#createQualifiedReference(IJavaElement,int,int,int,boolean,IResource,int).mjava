    public static TypeReference createQualifiedReference(
        IJavaElement enclosingElement,
        int accuracy,
        int start,
        int length,
        boolean insideDocComment,
        IResource resource,
        int simpleNameStart) {
      Assert.isTrue(start < simpleNameStart && simpleNameStart < start + length);
      return new TypeReference(
          enclosingElement,
          accuracy,
          start,
          length,
          insideDocComment,
          resource,
          simpleNameStart,
          null);
    }

