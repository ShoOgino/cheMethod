  /*
   * @see ASTVisitor#visit(ArrayInitializer)
   */
  @Override
  public boolean visit(ArrayInitializer node) {
    this.fBuffer.append("{"); // $NON-NLS-1$
    for (Iterator<Expression> it = node.expressions().iterator(); it.hasNext(); ) {
      Expression e = it.next();
      e.accept(this);
      if (it.hasNext()) {
        this.fBuffer.append(","); // $NON-NLS-1$
      }
    }
    this.fBuffer.append("}"); // $NON-NLS-1$
    return false;
  }

