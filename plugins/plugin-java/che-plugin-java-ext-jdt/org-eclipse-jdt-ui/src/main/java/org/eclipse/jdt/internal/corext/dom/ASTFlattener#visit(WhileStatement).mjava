  /*
   * @see ASTVisitor#visit(WhileStatement)
   */
  @Override
  public boolean visit(WhileStatement node) {
    this.fBuffer.append("while ("); //$NON-NLS-1$
    node.getExpression().accept(this);
    this.fBuffer.append(") "); //$NON-NLS-1$
    node.getBody().accept(this);
    return false;
  }

