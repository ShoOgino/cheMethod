  /*
   * @see ASTVisitor#visit(ThisExpression)
   */
  @Override
  public boolean visit(ThisExpression node) {
    if (node.getQualifier() != null) {
      node.getQualifier().accept(this);
      this.fBuffer.append("."); //$NON-NLS-1$
    }
    this.fBuffer.append("this"); //$NON-NLS-1$
    return false;
  }

