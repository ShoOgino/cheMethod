  private void print(StringBuffer buffer, String tag, List<String> elements, boolean firstword) {
    if (!elements.isEmpty()) {
      buffer.append("<dt>"); // $NON-NLS-1$
      buffer.append(tag);
      buffer.append("</dt>"); // $NON-NLS-1$
      printDefinitions(buffer, elements, firstword);
    }
  }

