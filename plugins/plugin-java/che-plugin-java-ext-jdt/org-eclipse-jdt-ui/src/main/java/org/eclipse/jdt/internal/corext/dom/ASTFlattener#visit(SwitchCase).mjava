  /*
   * @see ASTVisitor#visit(SwitchCase)
   */
  @Override
  public boolean visit(SwitchCase node) {
    if (node.isDefault()) {
      this.fBuffer.append("default :"); //$NON-NLS-1$
    } else {
      this.fBuffer.append("case "); //$NON-NLS-1$
      node.getExpression().accept(this);
      this.fBuffer.append(":"); //$NON-NLS-1$
    }
    return false;
  }

