		private void copyInitializerToDestination(IInitializer initializer, CompilationUnitRewrite targetRewriter,
												  CompilationUnit sourceCuNode, CompilationUnit targetCuNode) throws JavaModelException {
			BodyDeclaration newInitializer = (BodyDeclaration)targetRewriter.getASTRewrite()
																			.createStringPlaceholder(getUnindentedSource(initializer),
																									 ASTNode.INITIALIZER);
			copyMemberToDestination(initializer, targetRewriter, sourceCuNode, targetCuNode, newInitializer);
		}

