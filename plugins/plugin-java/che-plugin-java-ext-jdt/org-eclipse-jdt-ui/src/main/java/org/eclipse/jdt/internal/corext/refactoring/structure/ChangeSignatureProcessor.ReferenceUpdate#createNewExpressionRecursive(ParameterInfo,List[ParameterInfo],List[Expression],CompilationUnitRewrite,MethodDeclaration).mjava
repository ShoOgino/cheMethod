    private Expression createNewExpressionRecursive(
        ParameterInfo info,
        List<ParameterInfo> parameterInfos,
        List<Expression> nodes,
        CompilationUnitRewrite cuRewrite,
        MethodDeclaration methodDeclaration) {
      if (fDefaultValueAdvisor != null && info.isAdded()) {
        return fDefaultValueAdvisor.createDefaultExpression(
            nodes, info, parameterInfos, methodDeclaration, true, cuRewrite);
      }
      return (Expression)
          getASTRewrite().createStringPlaceholder(info.getNewName(), ASTNode.METHOD_INVOCATION);
    }

