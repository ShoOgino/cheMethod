  private String printLiteral(String tagContent) {
    int contentStart = 0;
    for (int i = 0; i < tagContent.length(); i++) {
      if (!Character.isWhitespace(tagContent.charAt(i))) {
        contentStart = i;
        break;
      }
    }
    return HTMLPrinter.convertToHTMLContent(tagContent.substring(contentStart));
  }

