  /*
   * @see ASTVisitor#visit(Javadoc)
   */
  @Override
  public boolean visit(Javadoc node) {
    this.fBuffer.append("/** "); //$NON-NLS-1$
    for (Iterator<TagElement> it = node.tags().iterator(); it.hasNext(); ) {
      ASTNode e = it.next();
      e.accept(this);
    }
    this.fBuffer.append("\n */"); //$NON-NLS-1$
    return false;
  }

