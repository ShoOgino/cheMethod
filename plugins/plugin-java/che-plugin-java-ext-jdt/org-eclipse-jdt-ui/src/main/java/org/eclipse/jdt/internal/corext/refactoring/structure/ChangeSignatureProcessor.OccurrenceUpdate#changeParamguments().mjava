    protected final void changeParamguments() {
      for (Iterator<ParameterInfo> iter = getParameterInfos().iterator(); iter.hasNext(); ) {
        ParameterInfo info = iter.next();
        if (info.isAdded() || info.isDeleted()) continue;

        if (info.isRenamed()) changeParamgumentName(info);

        if (info.isTypeNameChanged()) changeParamgumentType(info);
      }
    }

