		public void handleException(Throwable exception) {
			if (fMulti == null) {
				fMulti = new MultiStatus(JavaPlugin.ID_PLUGIN, IStatus.OK, CorrectionMessages.JavaCorrectionProcessor_error_status, null);
			}
			fMulti.merge(new Status(IStatus.ERROR, JavaPlugin.ID_PLUGIN, IStatus.ERROR, CorrectionMessages
					.JavaCorrectionProcessor_error_status,
									exception));
		}

