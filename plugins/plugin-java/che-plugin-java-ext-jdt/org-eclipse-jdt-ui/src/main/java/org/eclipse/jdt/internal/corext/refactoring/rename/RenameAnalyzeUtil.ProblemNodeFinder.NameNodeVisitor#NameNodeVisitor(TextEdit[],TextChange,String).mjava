      public NameNodeVisitor(TextEdit[] edits, TextChange change, String key) {
        Assert.isNotNull(edits);
        Assert.isNotNull(key);

        fRanges =
            new HashSet<IRegion>(Arrays.asList(RefactoringAnalyzeUtil.getNewRanges(edits, change)));
