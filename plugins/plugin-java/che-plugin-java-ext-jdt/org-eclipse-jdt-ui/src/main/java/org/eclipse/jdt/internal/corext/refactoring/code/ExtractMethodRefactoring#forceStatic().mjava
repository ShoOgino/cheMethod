  private boolean forceStatic() {
    if (!fReplaceDuplicates) {
      return false;
    }
    for (int i = 0; i < fDuplicates.length; i++) {
      SnippetFinder.Match duplicate = fDuplicates[i];
      if (!duplicate.isInvalidNode() && duplicate.isNodeInStaticContext()) {
        return true;
      }
    }
    return false;
  }

