    private void doShutdown() {
        Iterator jobRecs = fSearchJobs.values().iterator();
        while (jobRecs.hasNext()) {
            SearchJobRecord element = (SearchJobRecord)jobRecs.next();
            if (element.job != null)
                element.job.cancel();
        }
