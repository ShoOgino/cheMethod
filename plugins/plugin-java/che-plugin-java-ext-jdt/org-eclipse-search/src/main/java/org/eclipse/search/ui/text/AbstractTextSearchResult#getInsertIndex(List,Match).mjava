  private static int getInsertIndex(List matches, Match match) {
    int count = matches.size();
    int min = 0, max = count - 1;
    while (min <= max) {
      int mid = (min + max) / 2;
      Match data = (Match) matches.get(mid);
      int compare = compare(match, data);
      if (compare > 0) max = mid - 1;
      else min = mid + 1;
    }
    return min;
  }

