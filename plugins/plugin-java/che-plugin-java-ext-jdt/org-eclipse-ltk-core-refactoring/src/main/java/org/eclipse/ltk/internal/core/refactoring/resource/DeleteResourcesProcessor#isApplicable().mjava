  /* (non-Javadoc)
   * @see org.eclipse.ltk.core.refactoring.participants.RefactoringProcessor#isApplicable()
   */
  public boolean isApplicable() throws CoreException {
    for (int i = 0; i < fResources.length; i++) {
      if (!canDelete(fResources[i])) {
        return false;
      }
    }
    return true;
  }

