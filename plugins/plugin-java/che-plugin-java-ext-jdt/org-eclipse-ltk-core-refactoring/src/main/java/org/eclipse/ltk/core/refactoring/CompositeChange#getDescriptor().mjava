	/**
	 * {@inheritDoc}
	 *
	 * @since 3.2
	 */
	public ChangeDescriptor getDescriptor() {
		for (final Iterator iterator= fChanges.iterator(); iterator.hasNext();) {
			final Change change= (Change) iterator.next();
			final ChangeDescriptor descriptor= change.getDescriptor();
			if (descriptor != null)
				return descriptor;
		}
		return null;
	}

