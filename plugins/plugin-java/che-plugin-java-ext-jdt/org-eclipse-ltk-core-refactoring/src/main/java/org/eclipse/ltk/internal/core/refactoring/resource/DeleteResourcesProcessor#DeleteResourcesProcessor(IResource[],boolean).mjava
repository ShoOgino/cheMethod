  /**
   * Create a new delete processor.
   *
   * @param resources the resources to delete. They can be either {@link IProject} or {@link IFile}
   *     and {@link IFolder}.
   * @param deleteContents <code>true</code> if this will delete the project contents. The content
   *     delete is not undoable.
   */
  public DeleteResourcesProcessor(IResource[] resources, boolean deleteContents) {
    fResources = removeDescendants(resources);
    fDeleteContents = deleteContents;
  }

