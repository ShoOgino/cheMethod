    protected boolean considerEdit(TextEdit edit) {
      if (fExcludes != null) {
        for (int i = 0; i < fExcludes.length; i++) {
          if (edit.equals(fExcludes[i])) return false;
        }
        return true;
      }
      if (fIncludes != null) {
        for (int i = 0; i < fIncludes.length; i++) {
          if (edit.equals(fIncludes[i])) return true;
        }
        return false;
      }
      return true;
    }

