    @Inject
    public ClasspathUpdaterServiceClientImpl(AsyncRequestFactory asyncRequestFactory,
                                             AppContext appContext,
                                             LoaderFactory loaderFactory) {
        this.asyncRequestFactory = asyncRequestFactory;
