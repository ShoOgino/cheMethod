    @Test
    public void changesShouldRevert() throws Exception {
        presenter.revertChanges();

        verify(view).clear();
        verify(view).addNode(Matchers.<NodeWidget>anyObject());
        verify(delegate, times(2)).onDirtyChanged();
        assertFalse(presenter.isDirty());
        verify(delegate, times(2)).onDirtyChanged();
    }

