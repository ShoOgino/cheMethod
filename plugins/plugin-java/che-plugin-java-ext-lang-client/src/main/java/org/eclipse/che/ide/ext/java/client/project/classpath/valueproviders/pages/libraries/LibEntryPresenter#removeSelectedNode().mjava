    @Override
    public void removeSelectedNode() {
        dirty = true;
        delegate.onDirtyChanged();
        view.removeNode(pageNodes.remove(selectedNode));
        if (!pageNodes.isEmpty()) {
            selectedNode = pageNodes.keySet().iterator().next();
            pageNodes.get(selectedNode).select();
        } else {
            selectedNode = null;
        }
    }

