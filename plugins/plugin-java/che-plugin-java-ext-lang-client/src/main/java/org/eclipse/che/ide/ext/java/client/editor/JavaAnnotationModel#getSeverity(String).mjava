    @Override
    public String getSeverity(String annotationType) {
        if (annotationType == null) {
            return "error";
        }
        switch (annotationType) {
            case "org.eclipse.jdt.ui.error":
                return "error";
            case "org.eclipse.jdt.ui.warning":
                return "warning";
            default:
                return "task";
        }
    }

