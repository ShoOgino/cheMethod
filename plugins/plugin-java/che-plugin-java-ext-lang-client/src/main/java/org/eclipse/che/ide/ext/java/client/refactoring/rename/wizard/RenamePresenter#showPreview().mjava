  private void showPreview() {
    getChanges()
        .then(
            refactoringResult -> {
              CheWorkspaceEdit edit = refactoringResult.getCheWorkspaceEdit();
              if (edit == null) {
                return;
              }
              previewPresenter.show(edit, this);
              previewPresenter.setTitle(locale.renameItemTitle());
            });
  }

