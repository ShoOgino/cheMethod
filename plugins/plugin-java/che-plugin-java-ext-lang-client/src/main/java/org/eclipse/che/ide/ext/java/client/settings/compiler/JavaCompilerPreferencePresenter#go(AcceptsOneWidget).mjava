  /** {@inheritDoc} */
  @Override
  public void go(AcceptsOneWidget container) {
    if (widgets.isEmpty()) {
      preferencesManager
          .loadPreferences()
          .then(
              properties -> {
                options.forEach(this::provideWidget);
                container.setWidget(view);
              });
    } else {
      container.setWidget(view);
    }
  }

