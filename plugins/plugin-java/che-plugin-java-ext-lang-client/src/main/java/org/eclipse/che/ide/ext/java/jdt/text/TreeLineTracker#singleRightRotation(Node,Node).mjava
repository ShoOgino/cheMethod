  /**
   * A right rotation around <code>parent</code>, whose structural position is replaced by <code>
   * node</code>.
   *
   * @param node the node moving up and right
   * @param parent the node moving right and down
   */
  private void singleRightRotation(Node node, Node parent) {
    rotateRight(parent);
    node.balance = 0;
    parent.balance = 0;
  }

