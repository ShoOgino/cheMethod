    public String getFqn() {
        String sourcePath = getSourceFolderPath(this);

        String fqn = "";

        String nodePath = getData().getPath();

        if (nodePath.startsWith(sourcePath + '/')) {
            fqn = nodePath.substring(sourcePath.length() + 1);
        }

        fqn = fqn.substring(0, fqn.indexOf('.'));

        return fqn.replace('/', '.');
    }

