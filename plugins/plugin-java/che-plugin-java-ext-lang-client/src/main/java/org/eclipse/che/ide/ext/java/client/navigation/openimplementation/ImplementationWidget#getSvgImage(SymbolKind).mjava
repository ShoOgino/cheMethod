  private SVGImage getSvgImage(SymbolKind symbolKind) {
    SVGResource icon;
    switch (symbolKind) {
      case Interface:
        icon = javaResources.interfaceItem();
        break;
      case Enum:
        icon = javaResources.enumItem();
        break;
      default:
        icon = javaResources.javaFile();
    }
    return new SVGImage(icon);
  }

