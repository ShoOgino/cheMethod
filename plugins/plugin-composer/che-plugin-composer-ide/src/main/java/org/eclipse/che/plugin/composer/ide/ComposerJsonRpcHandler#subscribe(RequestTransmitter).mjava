  @Inject
  private void subscribe(RequestTransmitter requestTransmitter) {
    if (isSubscribed) {
      return;
    }

    requestTransmitter
        .newRequest()
        .endpointId(WS_AGENT_JSON_RPC_ENDPOINT_ID)
        .methodName(COMPOSER_CHANNEL_SUBSCRIBE)
        .noParams()
        .sendAndSkipResult();

    isSubscribed = true;
  }

