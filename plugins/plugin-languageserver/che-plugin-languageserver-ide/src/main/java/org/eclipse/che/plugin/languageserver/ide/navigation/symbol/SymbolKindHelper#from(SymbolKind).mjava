  public String from(SymbolKind kind) {
    switch (kind) {
      case Method:
        return "method";
      case Function:
        return "function";
      case Constructor:
        return "constructor";
      case Variable:
        return "variable";
      case Class:
        return "class";
      case Interface:
        return "interface";
      case Namespace:
        return "namespace";
      case Package:
        return "package";
      case Module:
        return "module";
      case Property:
        return "property";
      case Enum:
        return "enum";
      case String:
        return "string";
      case File:
        return "file";
      case Array:
        return "array";
      case Number:
        return "number";
      case Boolean:
        return "boolean";
      default:
        return "property";
    }
  }

