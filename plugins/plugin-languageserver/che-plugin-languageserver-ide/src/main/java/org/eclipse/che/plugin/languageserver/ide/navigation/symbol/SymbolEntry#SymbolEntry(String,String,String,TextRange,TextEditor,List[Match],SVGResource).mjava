  public SymbolEntry(
      String name,
      String type,
      String description,
      TextRange range,
      TextEditor editor,
      List<Match> highlights,
      SVGResource icon) {
    this.name = name;
    this.type = type;
    this.description = description;
    this.range = range;
    this.editor = editor;
    this.icon = icon;
    setHighlights(highlights);
  }

