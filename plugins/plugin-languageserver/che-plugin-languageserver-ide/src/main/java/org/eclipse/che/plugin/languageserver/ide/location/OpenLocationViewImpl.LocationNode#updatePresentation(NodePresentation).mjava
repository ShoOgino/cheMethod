    @Override
    public void updatePresentation(@NotNull NodePresentation presentation) {
      presentation.setPresentableText(location.getLocation().getUri());
      presentation.setInfoText(
          "From:"
              + (location.getLocation().getRange().getStart().getLine() + 1)
              + ":"
              + (location.getLocation().getRange().getStart().getCharacter() + 1)
              + " To:"
              + (location.getLocation().getRange().getEnd().getLine() + 1)
              + ":"
              + (location.getLocation().getRange().getEnd().getCharacter() + 1));
    }

