    private List<Match> join(Match match, List<Match> result) {
        if (result.isEmpty()) {
            result.add(match);
        } else if (match.getEnd() == result.get(0).getStart()) {
            result.get(0).setStart(match.getStart());
        } else {
            result.add(0, match);
        }
        return result;
    }

