  @Inject
  public ChangeNode(
      TreeStyles styles,
      LanguageServerResources resources,
      EditorAgent editorAgent,
      AppContext appContext,
      @Assisted RenameChange change) {
    this.styles = styles;
    this.resources = resources;
    this.editorAgent = editorAgent;
    this.appContext = appContext;
    this.change = change;
  }

