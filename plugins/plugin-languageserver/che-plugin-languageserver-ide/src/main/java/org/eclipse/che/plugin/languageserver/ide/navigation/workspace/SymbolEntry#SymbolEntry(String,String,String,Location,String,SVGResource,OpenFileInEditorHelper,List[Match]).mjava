  public SymbolEntry(
      String name,
      String parameters,
      String description,
      Location symbolLocation,
      String type,
      SVGResource icon,
      OpenFileInEditorHelper editorHelper,
      List<Match> matches) {
    this.name = name;
    this.parameters = parameters;
    this.description = description;
    this.editorHelper = editorHelper;
    this.type = type;
    this.icon = icon;
    this.symbolLocation = symbolLocation;
    setHighlights(matches);
  }

