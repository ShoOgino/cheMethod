    public SymbolEntry(String name,
                       String parameters,
                       String description,
                       String filePath,
                       String type,
                       TextRange range,
                       SVGResource icon,
                       OpenFileInEditorHelper editorHelper, List<Match> matches) {
        super(editorHelper);
        this.name = name;
        this.parameters = parameters;
        this.description = description;
        this.filePath = filePath;
        this.type = type;
        this.range = range;
        this.icon = icon;
        setHighlights(matches);
    }

