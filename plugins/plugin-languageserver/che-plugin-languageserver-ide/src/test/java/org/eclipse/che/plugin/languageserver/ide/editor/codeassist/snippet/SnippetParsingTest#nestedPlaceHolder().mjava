  @Test
  public void nestedPlaceHolder() {
    testParseAndRender(
        "${1:Gurke $2}",
        e(
            Snippet.class,
            e(Placeholder.class, e(Snippet.class, e(Text.class), e(Placeholder.class)))));
    testParseAndRender(
        "$1:Gurke ${2:3}",
        e(
            Snippet.class,
            e(Placeholder.class),
            e(Text.class),
            e(Placeholder.class, e(Snippet.class, e(Text.class)))));
  }

