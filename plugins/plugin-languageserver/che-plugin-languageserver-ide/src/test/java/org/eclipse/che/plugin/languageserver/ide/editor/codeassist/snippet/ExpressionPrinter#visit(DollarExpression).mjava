  @Override
  public void visit(DollarExpression e) {
    try {
      out.write('$');
      if (e.isBrace()) {
        out.write('{');
      }
      e.getValue().accept(this);
      if (e.isBrace()) {
        out.write('}');
      }
    } catch (IOException e1) {
      throw new RuntimeException(e1);
    }
  }

