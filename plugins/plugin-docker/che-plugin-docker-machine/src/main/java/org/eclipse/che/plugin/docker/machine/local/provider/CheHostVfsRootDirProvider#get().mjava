    @Override
    public String get() {
        if (SystemInfo.isWindows()) {
            try {
                final Path cheHome = WindowsHostUtils.ensureCheHomeExist();
                final Path vfs = cheHome.resolve("vfs");
                vfs.toFile().mkdir();
                return vfs.toString();
            } catch (IOException e) {
                LOG.warn(e.getMessage());
                throw new RuntimeException(e);
            }
        } else {
            return fsRootDir;
        }
    }

