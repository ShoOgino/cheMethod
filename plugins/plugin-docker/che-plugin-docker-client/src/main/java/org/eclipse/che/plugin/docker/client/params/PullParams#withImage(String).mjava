    /**
     * Adds image to this parameters.
     *
     * @param image
     *         name of the image to pull
     * @return this params instance
     * @throws NullPointerException
     *         if {@code image} is null
     */
    public PullParams withImage(@NotNull String image) {
        requireNonNull(image);
        this.image = image;
        return this;
    }

