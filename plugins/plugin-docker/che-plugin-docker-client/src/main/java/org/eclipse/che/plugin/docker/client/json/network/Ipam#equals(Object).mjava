    @Override
    public boolean equals(Object o) {
        if (this == o) return true;
        if (!(o instanceof Ipam)) return false;
        Ipam ipam = (Ipam)o;
        return Objects.equals(driver, ipam.driver) &&
               Objects.equals(config, ipam.config) &&
               Objects.equals(options, ipam.options);
    }

