    @Override
    public String toString() {
        return "ContainerState{" +
               "running=" + running +
               ", pid=" + pid +
               ", exitCode=" + exitCode +
               ", startedAt='" + startedAt + '\'' +
               ", finishedAt='" + finishedAt + '\'' +
               ", paused=" + paused +
               ", restarting=" + restarting +
               ", dead=" + dead +
               ", oOMKilled=" + oOMKilled +
               ", error='" + error + '\'' +
               '}';
    }

