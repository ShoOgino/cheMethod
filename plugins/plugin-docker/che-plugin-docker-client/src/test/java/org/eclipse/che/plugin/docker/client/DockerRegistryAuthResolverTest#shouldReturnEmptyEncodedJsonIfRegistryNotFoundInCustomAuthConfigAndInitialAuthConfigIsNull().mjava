    @Test
    public void shouldReturnEmptyEncodedJsonIfRegistryNotFoundInCustomAuthConfigAndInitialAuthConfigIsNull() {
        when(initialAuthConfig.getAuthConfigs()).thenReturn(emptyAuthConfigs);

        String base64HeaderValue = authResolver.getXRegistryAuthHeaderValue(UNKNOWN_REGISTRY, customAuthConfigs);

        assertEqualsXRegistryAuthHeader(base64ToAuthConfig(base64HeaderValue),
                                        jsonToAuthConfig(EMPTY_JSON));
    }

