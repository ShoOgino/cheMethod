  @Override
  public Promise<Void> branchDelete(Path project, String name, boolean force) {
    String url =
        getWsAgentBaseUrl()
            + BRANCH
            + "?projectPath="
            + encodePath(project)
            + "&name="
            + name
            + "&force="
            + force;
    return asyncRequestFactory.createDeleteRequest(url).loader(loader).send();
  }

