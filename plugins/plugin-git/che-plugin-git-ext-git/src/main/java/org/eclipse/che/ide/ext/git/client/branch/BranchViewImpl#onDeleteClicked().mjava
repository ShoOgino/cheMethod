  private void onDeleteClicked() {
    dialogFactory
        .createConfirmDialog(
            locale.branchDelete(),
            locale.branchDeleteAsk(branches.getSelectionModel().getSelectedItem().getName()),
            new ConfirmCallback() {
              @Override
              public void accepted() {
                delegate.onDeleteClicked();
              }
            },
            null)
        .show();
  }

