  @Override
  public Promise<String> diff(
      Path project,
      List<String> files,
      DiffType type,
      boolean noRenames,
      int renameLimit,
      String commitA,
      boolean cached) {
    return diff(project, files, type, noRenames, renameLimit, commitA, null, cached)
        .send(new StringUnmarshaller());
  }

