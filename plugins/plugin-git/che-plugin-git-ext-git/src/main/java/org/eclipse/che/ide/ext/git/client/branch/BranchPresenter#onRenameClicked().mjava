    @Override
    public void onRenameClicked() {
        if (selectedBranch.isRemote()) {
            dialogFactory.createConfirmDialog(constant.branchConfirmRenameTitle(),
                                              constant.branchConfirmRenameMessage(),
                                              this::renameBranch,
                                              null).show();
        } else {
            renameBranch();
        }
    }

