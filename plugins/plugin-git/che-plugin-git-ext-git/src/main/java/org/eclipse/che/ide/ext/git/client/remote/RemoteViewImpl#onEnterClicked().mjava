    @Override
    protected void onEnterClicked() {
        if (isWidgetFocused(btnClose)) {
            delegate.onCloseClicked();
            return;
        }

        if (isWidgetFocused(btnAdd)) {
            delegate.onAddClicked();
            return;
        }

        if (isWidgetFocused(btnDelete)) {
            onDeleteClicked();
        }
    }

