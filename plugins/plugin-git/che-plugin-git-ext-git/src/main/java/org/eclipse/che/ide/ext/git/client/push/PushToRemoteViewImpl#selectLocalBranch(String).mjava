  /** {@inheritDoc} */
  @Override
  public void selectLocalBranch(@NotNull String branch) {
    for (int i = 0; i < localBranch.getItemCount(); i++) {
      if (localBranch.getValue(i).equals(branch)) {
        localBranch.setItemSelected(i, true);
        delegate.onLocalBranchChanged();
        break;
      }
    }
  }

