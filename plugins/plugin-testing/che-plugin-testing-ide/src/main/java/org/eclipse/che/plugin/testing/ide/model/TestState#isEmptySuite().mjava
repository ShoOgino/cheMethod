  public boolean isEmptySuite() {

    if (!isSuite()) {
      return true;
    }
    for (TestState testState : getChildren()) {
      if (testState.isSuite()) {
        if (!testState.isEmptySuite()) {
          return false;
        }
      } else {
        return false;
      }
    }

    return true;
  }

