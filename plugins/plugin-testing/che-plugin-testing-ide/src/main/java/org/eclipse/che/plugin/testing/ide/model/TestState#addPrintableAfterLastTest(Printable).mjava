    protected void addPrintableAfterLastTest(Printable printable) {
        if (treeBuildBeforeStart) {
            int index = 0;
            for (Printable prnt : printables) {
                if (prnt instanceof TestState && !((TestState) prnt).isFinal()) {
                    break;
                }
                index++;
            }

            insert(printable, index);
        } else {
            addLast(printable);
        }
    }

