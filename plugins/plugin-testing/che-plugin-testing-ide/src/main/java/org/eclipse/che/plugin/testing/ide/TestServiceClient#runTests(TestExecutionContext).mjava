    public JsonRpcPromise<TestLaunchResult> runTests(TestExecutionContext context) {
        return requestTransmitter.newRequest()
                                 .endpointId("ws-agent")
                                 .methodName(Constants.RUN_TESTS_METHOD)
                                 .paramsAsDto(context)
                                 .sendAndReceiveResultAsDto(TestLaunchResult.class);
    }

