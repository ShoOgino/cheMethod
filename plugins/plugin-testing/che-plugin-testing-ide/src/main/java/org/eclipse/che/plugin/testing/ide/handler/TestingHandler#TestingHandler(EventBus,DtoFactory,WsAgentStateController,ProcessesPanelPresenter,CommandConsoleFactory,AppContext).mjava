                          DtoFactory factory,
                          WsAgentStateController wsAgentStateController,
                          ProcessesPanelPresenter processesPanelPresenter,
                          CommandConsoleFactory commandConsoleFactory,
                          AppContext appContext) {
        this.eventBus = eventBus;
        this.factory = factory;
