  public void testSelected(TestState testState) {
    if (currentTest == testState) {
      return;
    }
    if (currentTest != null) {
      currentTest.setPrinter(null);
    }
    if (testState == null) {
      clearConsole();
      return;
    }
    currentTest = testState;
    clearConsole();
    testState.setPrinter(this);
    testState.print(this);
  }

