    @Override
    public void print(Printer printer) {
        printer.print(Printer.NEW_LINE, Printer.OutputType.STDERR);
        printer.print(errorMessage, Printer.OutputType.STDERR);

        printer.print(Printer.NEW_LINE, Printer.OutputType.STDERR);
        printer.print(stackTrace, Printer.OutputType.STDERR);
        printer.print(Printer.NEW_LINE, Printer.OutputType.STDERR);
    }

