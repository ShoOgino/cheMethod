  /** Ensures that project api has been initialized only once. */
  public static void ensure() throws Exception {
    if (!initialized.get()) {
      synchronized (initialized) {
        if (!initialized.get()) {
          try {
            init();
          } catch (Exception e) {
            LOG.error(e.getMessage(), e);
          }
          initialized.set(true);
        }
      }
    }
  }

