  /** {@inheritDoc} */
  @Override
  public void onDeleteClicked(@NotNull final YamlPreference pairKey) {
    dialogFactory
        .createConfirmDialog(
            local.deleteUrlText(),
            local.deleteUrlLabel(),
            new ConfirmCallback() {
              @Override
              public void accepted() {
                deleteKeyFromPreferences(pairKey);
                refreshTable();
                nowDirty();
              }
            },
            getCancelCallback())
        .show();
  }

