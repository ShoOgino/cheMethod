  /** {@inheritDoc} */
  @Override
  public void onAddUrlClicked() {
    dialogFactory
        .createInputDialog(
            local.addUrlText(),
            local.addUrlLabel(),
            "",
            0,
            0,
            local.addSchemaButtonText(),
            new InputCallback() {
              @Override
              public void accepted(String url) {
                addUrlToPreferences(url);
                refreshTable();
                nowDirty();
              }
            },
            getCancelCallback())
        .show();
  }

