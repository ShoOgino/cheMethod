    /**
     * {@inheritDoc}
     */
    @Override
    public void onOkClicked() {
        final String projectPath = getCurrentProjectPath();

        if (projectPath == null) {
            return;
        }

        view.onClose();

        if (view.isEditPropertySelected()) {
            editProperty(projectPath);
        } else if (view.isDeletePropertySelected()) {
            deleteProperty(projectPath);
        }
    }

