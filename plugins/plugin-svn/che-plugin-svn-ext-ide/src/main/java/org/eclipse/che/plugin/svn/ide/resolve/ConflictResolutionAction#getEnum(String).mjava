  public static ConflictResolutionAction getEnum(String text) {
    switch (text) {
      case "postpone":
        return POSTPONE;
      case "base":
        return BASE;
      case "working":
        return WORKING;
      case "mine-full":
        return MINE_FULL;
      case "theirs-full":
        return THEIRS_FULL;
      case "mine-conflict":
        return MINE_CONFLICT;
      case "theirs-conflict":
        return THEIRS_CONFLICT;
      default:
        return null;
    }
  }

