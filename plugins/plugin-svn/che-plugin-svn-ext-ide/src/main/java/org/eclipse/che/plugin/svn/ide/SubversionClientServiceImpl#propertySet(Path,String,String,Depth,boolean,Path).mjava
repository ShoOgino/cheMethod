  @Override
  public Promise<CLIOutputResponse> propertySet(
      Path project,
      String propertyName,
      String propertyValues,
      Depth depth,
      boolean force,
      Path path) {
    final PropertyRequest request =
        dtoFactory
            .createDto(PropertySetRequest.class)
            .withValue(propertyValues)
            .withProjectPath(project.toString())
            .withName(propertyName)
            .withDepth(depth)
            .withForce(force)
            .withPath(path.toString());

    return asyncRequestFactory
        .createPostRequest(getBaseUrl() + "/propset", request)
        .loader(loader)
        .send(dtoUnmarshallerFactory.newUnmarshaller(CLIOutputResponse.class));
  }

