  @Inject
  public AddAction(
      AppContext appContext,
      SubversionExtensionLocalizationConstants constants,
      SubversionExtensionResources resources,
      AddPresenter presenter) {
    super(
        constants.addTitle(),
        constants.addDescription(),
        resources.add(),
        appContext,
        constants,
        resources);
    this.presenter = presenter;
  }

