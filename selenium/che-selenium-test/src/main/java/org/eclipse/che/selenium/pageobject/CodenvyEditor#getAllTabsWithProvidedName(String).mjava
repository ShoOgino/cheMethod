  private List<WebElement> getAllTabsWithProvidedName(String tabName) {
    return webDriverWaitFactory
        .get()
        .until(
            visibilityOfAllElementsLocatedBy(
                By.xpath(
                    format(
                        "//div[@id='gwt-debug-multiSplitPanel-tabsPanel']//div[text()='%s']",
                        tabName))));
  }

