  /**
   * select sample into Samples list with click
   *
   * @param sample
   */
  public void selectSample(String sample) {
    waitSample(sample);
    loader.waitOnClosed();
    seleniumWebDriver
        .findElement(By.xpath(String.format(Locators.SAMPLES_SELECTOR_BY_FORMAT_STRING, sample)))
        .click();
  }

