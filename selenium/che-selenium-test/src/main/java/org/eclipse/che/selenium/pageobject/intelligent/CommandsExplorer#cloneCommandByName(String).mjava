  public void cloneCommandByName(String commandName) {
    loader.waitOnClosed();
    new WebDriverWait(seleniumWebDriver, LOAD_PAGE_TIMEOUT_SEC)
        .until(
            ExpectedConditions.elementToBeClickable(
                By.xpath(
                    "//div[@id='command_"
                        + commandName
                        + "']//span[@id='commands_tree-button-duplicate']")))
        .click();
  }

