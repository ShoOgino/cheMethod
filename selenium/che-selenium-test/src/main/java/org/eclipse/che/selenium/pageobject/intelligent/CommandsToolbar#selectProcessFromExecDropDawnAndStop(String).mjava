  /**
   * expand exec dropdawn list
   *
   * @param commandName
   */
  public void selectProcessFromExecDropDawnAndStop(String commandName) {
    clickOnExecDropDawn();
    appearanceWait
        .until(
            ExpectedConditions.elementToBeClickable(
                By.xpath(String.format(Locators.EXEC_COMMAND_SELECTOR, commandName))))
        .click();
  }

