  public void waitAddOrImportFormClosed() {
    seleniumWebDriverHelper.waitAllInvisibilityBy(
        asList(
            By.id(SAMPLES_BUTTON_ID),
            By.id(BLANK_BUTTON_ID),
            By.id(GIT_BUTTON_ID),
            By.id(GITHUG_BUTTON_ID),
            By.id(ZIP_BUTTON_ID),
            By.id(ADD_BUTTON_ID),
            By.id(CANCEL_BUTTON_ID)));
  }

