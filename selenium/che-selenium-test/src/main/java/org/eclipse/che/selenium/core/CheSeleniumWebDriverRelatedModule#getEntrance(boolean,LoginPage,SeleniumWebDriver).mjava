  @Provides
  public Entrance getEntrance(
      @Named(CHE_MULTIUSER) boolean isMultiuser,
      LoginPage loginPage,
      SeleniumWebDriver seleniumWebDriver) {
    if (isMultiuser) {
      return new LoginPageEntrance(loginPage);
    } else {
      return new CookieEntrance(seleniumWebDriver);
    }
  }

