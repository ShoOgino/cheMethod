  public List<String> getAllBreakpoints() {
    return Stream.of(
            seleniumWebDriver
                .wait(REDRAW_UI_ELEMENTS_TIMEOUT_SEC)
                .until(visibilityOfElementLocated(id(BreakpointsPanel.ID)))
                .getText()
                .split("\n"))
        .filter(s -> !s.equals("?"))
        .collect(Collectors.toList());
  }

