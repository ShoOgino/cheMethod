  public void waitGreenNode(String path) {
    redrawUiElementsWait.until(
        ExpectedConditions.visibilityOfElementLocated(
            By.xpath(
                String.format(
                    "//div[@id='gwt-debug-projectTree']//div[@path='/%s']/descendant::div[@style='%s']",
                    path, "color: #72ad42;"))));
  }

