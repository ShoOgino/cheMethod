  /** Deletes current line with Ctrl+D and inserts new line instead */
  public void deleteCurrentLineAndInsertNew() {
    actionsFactory
        .createAction(seleniumWebDriver)
        .keyDown(CONTROL)
        .sendKeys("d")
        .keyUp(CONTROL)
        .sendKeys(ARROW_UP)
        .sendKeys(END)
        .sendKeys(ENTER)
        .perform();
  }

