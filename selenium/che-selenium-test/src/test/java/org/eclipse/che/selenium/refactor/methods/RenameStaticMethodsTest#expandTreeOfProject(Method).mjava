  @BeforeMethod
  public void expandTreeOfProject(Method testName) {
    try {
      loader.waitOnClosed();
      if (refactor.isWidgetOpened()) {
        refactor.clickCancelButtonRefactorForm();
      }
      setFieldsForTest(testName.getName());
    } catch (Exception e) {
      LOG.error(e.getLocalizedMessage(), e);
    }
  }

