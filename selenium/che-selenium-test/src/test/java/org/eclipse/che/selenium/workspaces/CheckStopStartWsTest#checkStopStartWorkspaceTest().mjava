  @Test
  public void checkStopStartWorkspaceTest() {
    ide.waitOpenedWorkspaceIsReadyToUse();
    notificationsPopupPanel.waitPopupPanelsAreClosed();

    menu.runCommand(WORKSPACE, STOP_WORKSPACE);
