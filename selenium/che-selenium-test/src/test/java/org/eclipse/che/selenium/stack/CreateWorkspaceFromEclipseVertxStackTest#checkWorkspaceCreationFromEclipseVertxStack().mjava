    testWorkspace =
        createWorkspaceHelper.createWorkspaceFromStackWithProjects(
            ECLIPSE_VERTX, WORKSPACE_NAME, projects);

    currentWindow = ide.switchToIdeAndWaitWorkspaceIsReadyToUse();
    consoles.waitJDTLSProjectResolveFinishedMessage(
