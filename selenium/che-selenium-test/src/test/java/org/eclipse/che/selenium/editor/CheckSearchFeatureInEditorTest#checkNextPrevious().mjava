    searchReplacePanel.clickOnNextBtn();
    editor.waitSpecifiedValueForLineAndChar(SECOND_CURSOR_POSITION);
    searchReplacePanel.clickOnPreviousBtn();
    editor.waitSpecifiedValueForLineAndChar(FIRST_CURSOR_POSITION);
  }

