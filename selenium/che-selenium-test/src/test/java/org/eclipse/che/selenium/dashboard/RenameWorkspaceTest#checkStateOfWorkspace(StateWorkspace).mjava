  public boolean checkStateOfWorkspace(StateWorkspace stateWorkspace) {
    try {
      dashboard
          .driver()
          .findElement(
              By.xpath(
                  format(
                      "//div[contains(@class, 'workspace-status')]/span[text()='%s']",
                      stateWorkspace.getStatus())));
      return true;
    } catch (NoSuchElementException e) {
      return false;
    }
  }

