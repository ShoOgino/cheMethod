    @Override
    public boolean equals(Object obj) {
        if (this == obj) {
            return true;
        }
        if (!(obj instanceof AclEntryImpl)) {
            return false;
        }
        final AclEntryImpl other = (AclEntryImpl)obj;
        return Objects.equals(user, other.user)
               && actions.equals(other.actions);
    }

