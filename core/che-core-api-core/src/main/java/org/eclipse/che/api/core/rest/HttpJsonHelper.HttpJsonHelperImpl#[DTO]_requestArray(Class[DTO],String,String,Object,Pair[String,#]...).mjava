    public <DTO> List<DTO> requestArray(
        Class<DTO> dtoInterface,
        String url,
        String method,
        Object body,
        Pair<String, ?>... parameters)
        throws IOException, ServerException, UnauthorizedException, ForbiddenException,
            NotFoundException, ConflictException {
      return requestArray(dtoInterface, -1, url, method, body, parameters);
    }

