    public SystemPropertiesHelper restoreFromBackup() {
        for (Map.Entry<String, String> entry : backup.entrySet()) {
            if (entry.getValue() == null) {
                System.clearProperty(entry.getKey());
            } else {
                System.setProperty(entry.getKey(), entry.getValue());
            }
        }
        backup.clear();
        return this;
    }

