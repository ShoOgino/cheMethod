    @Override
    public V get(K key) {
        V value = protectedSegment.get(key);
        if (value != null) {
            protectedHits++;
            return value;
        }
        value = probationarySegment.remove(key);
        if (value == null) {
            misses++;
            return null;
        }
        probationaryHits++;
        protectedSegment.put(key, value);
        return value;
    }

