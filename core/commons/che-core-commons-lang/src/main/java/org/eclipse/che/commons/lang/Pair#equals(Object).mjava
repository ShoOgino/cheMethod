    @Override
    public boolean equals(Object o) {
        if (this == o) {
            return true;
        }
        if (!(o instanceof Pair<?, ?>)) {
            return false;
        }
        final Pair other = (Pair)o;
        return (first == null ? other.first == null : first.equals(other.first)) &&
               (second == null ? other.second == null : second.equals(other.second));
    }

