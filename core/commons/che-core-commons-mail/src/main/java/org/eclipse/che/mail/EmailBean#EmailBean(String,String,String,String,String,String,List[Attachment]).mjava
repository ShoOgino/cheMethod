  public EmailBean(
      String from,
      String to,
      String replyTo,
      String mimeType,
      String body,
      String subject,
      List<Attachment> attachments) {
    this.from = from;
    this.to = to;
    this.replyTo = replyTo;
    this.mimeType = mimeType;
    this.body = body;
    this.subject = subject;
    this.attachments = attachments;
  }

