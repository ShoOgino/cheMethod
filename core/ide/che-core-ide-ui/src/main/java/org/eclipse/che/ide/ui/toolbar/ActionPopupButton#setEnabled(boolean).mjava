    /**
     * Set is enabled.
     *
     * @param enabled
     *         is enabled
     */
    public void setEnabled(boolean enabled) {
        this.enabled = enabled;

        if (enabled) {
            panel.getElement().removeClassName(toolbarResources.toolbar().disabled());
        } else {
            panel.getElement().addClassName(toolbarResources.toolbar().disabled());
        }
    }

