    @NotNull
    private String getDisplayType(@NotNull ResourceBasedNode<?> node) {
        if (node instanceof ProjectNode) {
            return "project";
        } else if (node instanceof FolderReferenceNode) {
            return "folder";
        } else if (node instanceof FileReferenceNode) {
            return "file";
        } else {
            return "resource";
        }
    }

