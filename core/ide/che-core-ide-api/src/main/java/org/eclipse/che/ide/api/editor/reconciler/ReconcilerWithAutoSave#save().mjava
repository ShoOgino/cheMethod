    private void save() {
        if (autoSaveEnabled) {
            editor.doSave(new AsyncCallback<EditorInput>() {
                @Override
                public void onFailure(Throwable throwable) {
                    Log.error(ReconcilerWithAutoSave.class, throwable);
                }

                @Override
                public void onSuccess(EditorInput editorInput) {
                    processNextRegion();

                }
            });
        } else {
            processNextRegion();
        }
    }

