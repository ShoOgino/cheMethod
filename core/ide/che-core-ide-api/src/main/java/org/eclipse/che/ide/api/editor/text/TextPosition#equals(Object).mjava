    @Override
    public boolean equals(final Object obj) {
        if (this == obj) {
            return true;
        }
        if (obj == null) {
            return false;
        }
        if (!(obj instanceof TextPosition)) {
            return false;
        }
        final TextPosition other = (TextPosition)obj;
        if (character != other.character) {
            return false;
        }
        if (line != other.line) {
            return false;
        }
        return true;
    }

