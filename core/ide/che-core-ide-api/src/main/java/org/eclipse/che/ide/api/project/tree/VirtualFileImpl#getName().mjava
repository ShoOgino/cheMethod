    @Override
    public String getName() {
        if (fileInfo.getName() == null) {
            String path = fileInfo.getPath();
            if (path == null) {
                throw new UnsupportedOperationException();
            } else {
                return path.substring(path.lastIndexOf("/") + 1);
            }
        } else {
            return fileInfo.getName();
        }
    }

