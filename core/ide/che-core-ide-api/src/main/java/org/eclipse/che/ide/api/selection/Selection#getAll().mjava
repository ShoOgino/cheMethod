    /**
     * Returns all the selected elements.
     * 
     * @return all the selected elements.
     * @deprecated use {@link #getAllElements()}
     */
    @Deprecated
    public List<T> getAll() {
        final List<T> copy = new ArrayList<>();
        for (final T item : this.elements) {
            copy.add(item);
        }
        return copy;
    }

