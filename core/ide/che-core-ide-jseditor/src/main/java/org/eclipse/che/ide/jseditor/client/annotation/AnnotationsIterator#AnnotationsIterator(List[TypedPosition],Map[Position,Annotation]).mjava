    /**
     * @param positions
     *         positions to iterate over
     * @param map
     *         a map to map positions to annotations
     */
    public AnnotationsIterator(final List<TypedPosition> positions,
                               final Map<Position, Annotation> map) {
        this.positions = positions;
        this.index = 0;
        this.map = map;
        next = findNext();
    }

