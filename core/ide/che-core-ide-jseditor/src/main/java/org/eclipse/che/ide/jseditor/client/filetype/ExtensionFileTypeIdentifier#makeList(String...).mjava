    /**
     * Builds a list from the parameters.
     *
     * @param strings
     *         the elements of the list
     * @return the list
     */
    private List<String> makeList(final String... strings) {
        final List<String> result = new ArrayList<>();
        for (String string : strings) {
            result.add(string);
        }
        return result;
    }

