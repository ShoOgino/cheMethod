        @Override
        @SuppressWarnings("unchecked")
        public <T> TypeAdapter<T> create(Gson gson, TypeToken<T> type) {
            DtoProvider<?> prov = dtoInterface2Providers.get(type.getRawType());
            if (prov != null) {
                return (TypeAdapter<T>) gson.getAdapter(prov.getImplClass());
            }
            return null;
        }

