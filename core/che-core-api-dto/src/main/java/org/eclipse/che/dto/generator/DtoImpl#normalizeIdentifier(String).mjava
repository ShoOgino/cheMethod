    private String normalizeIdentifier(String fieldName) {
        // use $ prefix according to http://docs.oracle.com/javase/specs/jls/se7/html/jls-3.html#jls-3.8
        switch (fieldName) {
            case "default":
                fieldName = "$" + fieldName;
                break;
            // add other keywords here
        }
        return fieldName;
    }

