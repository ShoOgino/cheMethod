    /** @see GitCommand#execute() */
    @Override
    public Void execute() throws GitException {
        if (oldName == null || newName == null) {
            throw new GitException("Old name or new name was not set.");
        }
        reset();

        if (remote == null) {
            renameLocalBranch();
        } else {
            renameRemoteBranch();
        }
        return null;
    }

