    /** @see GitCommand#execute() */
    @Override
    public Void execute() throws GitException {
        if (listOfItems == null) {
            throw new GitException("Nothing to remove.");
        }
        reset();
        commandLine.add("rm");
        commandLine.add(listOfItems);
        if (cached) {
            commandLine.add("--cached");
        }
        if (recursively) {
            commandLine.add("-r");
        }
        start();
        return null;
    }

