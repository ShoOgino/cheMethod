    @Override
    public void onError() {
        if (countRetry > 0) {
            retryConnectionTimer.schedule(1000);
        } else {
            state = WsAgentState.STOPPED;
            initialLoadingInfo.setOperationStatus(WS_AGENT_BOOTING.getValue(), ERROR);
            loader.hide();
            eventBus.fireEvent(WsAgentStateEvent.createWsAgentStoppedEvent());
        }
    }

