    @Override
    public boolean equals(Object obj) {
        if (this == obj) {
            return true;
        }
        if (!(obj instanceof GroupImpl)) {
            return false;
        }
        final GroupImpl other = (GroupImpl)obj;
        return Objects.equals(name, other.name) &&
               Objects.equals(unit, other.unit) &&
               getAcl().equals(other.getAcl());
    }

