    /**
     * Initialises Set of Project Type definitions
     *
     * @param types
     */
    @Inject
    public ProjectTypeRegistry(Set<ProjectTypeDef> types) {

        validate(types);

        for (ProjectTypeDef type : validatedData.values()) {

            try {
                init(type);

            } catch (ProjectTypeConstraintException e) {
                LOG.error(e.getMessage());
            }

        }
    }

