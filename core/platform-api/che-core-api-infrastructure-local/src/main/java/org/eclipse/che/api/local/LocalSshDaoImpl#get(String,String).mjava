    @Override
    public List<SshPairImpl> get(String owner, String service) {
        lock.readLock().lock();
        try {
            return pairs.get(owner)
                        .stream()
                        .filter(sshPair -> sshPair.getService().equals(service))
                        .collect(Collectors.toList());
        } finally {
            lock.readLock().unlock();
        }
    }

