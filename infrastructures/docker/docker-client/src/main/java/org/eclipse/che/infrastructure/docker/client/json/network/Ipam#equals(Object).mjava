  @Override
  public boolean equals(Object obj) {
    if (this == obj) {
      return true;
    }
    if (!(obj instanceof Ipam)) {
      return false;
    }
    final Ipam that = (Ipam) obj;
    return Objects.equals(driver, that.driver)
        && getConfig().equals(that.getConfig())
        && getOptions().equals(that.getOptions());
  }

