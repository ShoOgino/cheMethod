  @Override
  public String toString() {
    return "DockerfileEnvironment{"
        + "dockerfile='"
        + dockerfileContent
        + '\''
        + ", machines="
        + getMachines()
        + ", recipe="
        + getRecipe()
        + ", warnings="
        + getWarnings()
        + ", type="
        + getType()
        + ", attributes="
        + getAttributes()
        + '}';
  }

