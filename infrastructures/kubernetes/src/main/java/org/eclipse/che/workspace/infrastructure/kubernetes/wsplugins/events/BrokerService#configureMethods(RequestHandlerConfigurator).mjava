  @Inject
  public void configureMethods(RequestHandlerConfigurator requestHandler) {
    requestHandler
        .newConfiguration()
        .methodName(BROKER_STATUS_CHANGED_METHOD)
        .paramsAsDto(BrokerResultEvent.class)
        .noResult()
        .withConsumer(this::handle);

    requestHandler
        .newConfiguration()
        .methodName(BROKER_RESULT_METHOD)
        .paramsAsDto(BrokerResultEvent.class)
        .noResult()
        .withConsumer(this::handle);
  }

