  public KubernetesServerExposer(
      ExternalServerExposerStrategy<T> externalServerExposer,
      String machineName,
      Pod pod,
      Container container,
      T k8sEnv) {
    this.externalServerExposer = externalServerExposer;
    this.machineName = machineName;
    this.pod = pod;
    this.container = container;
    this.k8sEnv = k8sEnv;
  }

