  private DeployBroker getDeployBrokerPhase(
      String workspaceId,
      KubernetesNamespace kubernetesNamespace,
      KubernetesEnvironment brokerEnvironment,
      CompletableFuture<List<ChePlugin>> toolingFuture) {
