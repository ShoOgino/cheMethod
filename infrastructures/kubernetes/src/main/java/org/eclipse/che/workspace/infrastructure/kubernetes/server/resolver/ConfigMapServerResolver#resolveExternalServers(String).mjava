  @Override
  protected Map<String, ServerImpl> resolveExternalServers(String machineName) {
    return configMaps
        .get(machineName)
        .stream()
        .map(this::fillGatewayServers)
        .flatMap(m -> m.entrySet().stream())
        .collect(Collectors.toMap(Entry::getKey, Entry::getValue, (s1, s2) -> s2));
  }

