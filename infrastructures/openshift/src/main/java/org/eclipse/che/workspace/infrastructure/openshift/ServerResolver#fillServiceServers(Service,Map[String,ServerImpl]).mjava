  private void fillServiceServers(Service service, Map<String, ServerImpl> servers) {
    Annotations.newDeserializer(service.getMetadata().getAnnotations())
        .servers()
        .forEach(
            (name, config) ->
                servers.put(
                    name,
                    newServer(
                        config.getProtocol(),
                        service.getMetadata().getName(),
                        config.getPort(),
                        config.getPath(),
                        config.getAttributes())));
  }

